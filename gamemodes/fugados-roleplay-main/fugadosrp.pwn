cmd:arrestar(playerid, params[])
{
	if (!member_faccion(playerid, 1, 2, 4, 7)) return SendClientMessage(playerid, COLOR_GREY, "Sólo miembros de la ley policial.");
	if (!IsPlayerInRangeOfPoint(playerid, 7.0, -807.5030, -1755.6345, 46.5406)) return SendClientMessage(playerid, COLOR_GREY, "No estás en el punto de entrega.");
	new string[180], playerid2, time;
	if (sscanf(params, "dd", playerid2, time)) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /arrestar (JugadorID) (Minutos)");
	if (time < 10 || time > 300) { SendClientMessage(playerid, COLOR_GREY, "Tiempo: 10 a 300 minutos."); return 1; }
	if (!IsPlayerConnected(playerid2)) return SendClientMessage(playerid, COLOR_GREY, "Ese jugador no está conectado.");
	if (playerid == playerid2) return SendClientMessage(playerid, COLOR_GREY, "No puedes arrestarte a ti mismo, pendejo...");
	if (!CercaDelMarica(5.0, playerid, playerid2)) return SendClientMessage(playerid, COLOR_GREY, "Ese jugador no está cerca tuyo.");
	if (InfoPersonaje[playerid2][pEstrellas] < 1) return SendClientMessage(playerid, COLOR_GREY, "El sospechoso debe tener un nivel de búsqueda de al menos una estrella.");
	SetPlayerSpecialAction(playerid2, SPECIAL_ACTION_NONE);
	format(string, sizeof string, "[CENTRAL] %s fue arrestado por %s, %d minutos. (%s)", NombreJugador(playerid2), NombreJugador(playerid), time, InfoFaccion[InfoPersonaje[playerid][pMiembro]][fLogo]);
	SendRadioMessage(1, COLOR_RADIO, string);
	SendRadioMessage(2, COLOR_RADIO, string);
	SendRadioMessage(4, COLOR_RADIO, string);
	SendRadioMessage(7, COLOR_RADIO, string);
	InfoPersonaje[playerid2][pTiempoSancion] = time * 60;
	DeletePVar(playerid2, "IsFrozen");
	new rand = random(sizeof PrisonSpawns);
	SetPosEx(playerid2, PrisonSpawns[rand][0], PrisonSpawns[rand][1], PrisonSpawns[rand][2], 0.0000, 60, 60, 1);
	InfoPersonaje[playerid2][pSancionado] = 2;
	InfoPersonaje[playerid2][pTogTelefono] = 0;
	InfoPersonaje[playerid2][pArrestos] += 1;
	InfoPersonaje[playerid2][pEstrellas] = 0;
	SetPlayerWantedLevel(playerid2, 0);
	ClearAnimations(playerid2);
	RemovePlayerAttachedObject(playerid2, 0);
	SetPlayerSpecialAction(playerid2, SPECIAL_ACTION_NONE);
	DeletePVar(playerid2, "PlayerCuffed");
	QuitarBolsillos(playerid2);
	QuitarManos(playerid2);
	QuitarEspalda(playerid2);
	new query[410];
	mysql_format(db_samp, query, sizeof query, "UPDATE personajes SET Estrellas = 0, Sancionado = 2, TiempoSancion = '%d', Arrestos = '%d' WHERE ID = '%d'",InfoPersonaje[playerid2][pTiempoSancion], InfoPersonaje[playerid2][pArrestos], InfoPersonaje[playerid2][pID]);
	mysql_tquery(db_samp, query);
	SendClientMessageEx(playerid2, COLOR_LIGHTBLUE, "Fuiste encarcelado %d minutos. (Puedes verificar el tiempo restante en /cuenta)", time);
	return 1;
}

