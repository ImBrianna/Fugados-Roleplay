cmd:quitarllavelab(playerid, params[]){
	if(pDroga[playerid][LabID] == -1) return SendClientMessage(playerid, COLOR_GREY, "* No tienes llave de ningún laboratorio.");
	new id;

	if (sscanf(params, "i", id)) return SendClientMessage(playerid, COLOR_GREY, "Utiliza: /quitarllavelab ID");
	if(!IsPlayerConnectedEx(id)) return SendClientMessage(playerid, COLOR_GREY, "Este jugador no está conectado.");
	if(pDroga[id][LlaveLab] != pDroga[playerid][LabID]) return SendClientMessage(playerid, COLOR_GREY, "Este jugador no tiene una llave de tu laboratorio.");
	pDroga[id][LlaveLab] = -1;
	new labid = SQLLabID(pDroga[playerid][LabID]);
	if(Lab[labid][Llave1l] != -1){
		pDroga[id][LlaveLab] = -1;
		Lab[labid][Llave1l] = -1;
		guardar_int_mysql("personajes", "LlaveLab", InfoPersonaje[id][pID], -1);
	}else if(Lab[labid][Llave2l] != -1){
		pDroga[id][LlaveLab] = -1;
		Lab[labid][Llave2l] = -1;
		guardar_int_mysql("personajes", "LlaveLab", InfoPersonaje[id][pID], -1);
	}else if(Lab[labid][Llave3l] != -1){
		pDroga[id][LlaveLab] = -1;
		guardar_int_mysql("personajes", "LlaveLab", InfoPersonaje[id][pID], -1);
		Lab[labid][Llave3l] = -1;
	}
	SendClientMessageEx(playerid, COLOR_GREY, "Le has quitado una llave de laboratorio a %s.", NombreOriginal(id));
	ActualizarLaboratorio(labid);
	return 1;
}

cmd:darllavelab(playerid, params[]){
	if(pDroga[playerid][LabID] == -1) return SendClientMessage(playerid, COLOR_GREY, "* No tienes llave de ningún laboratorio.");
	new id;

	if (sscanf(params, "i", id)) return SendClientMessage(playerid, COLOR_GREY, "Utiliza: /darllavelab ID");
	if(!IsPlayerConnectedEx(id)) return SendClientMessage(playerid, COLOR_GREY, "Este jugador no está conectado.");
	if(pDroga[id][LlaveLab] != -1) return SendClientMessage(playerid, COLOR_GREY, "Este jugador ya tiene una llave de laboratorio.");
	new labid = SQLLabID(pDroga[playerid][LabID]);
	if(Lab[labid][Llave1l] == -1){
		pDroga[id][LlaveLab] = pDroga[playerid][LabID];
		Lab[labid][Llave1l] = InfoPersonaje[id][pID];
		guardar_int_mysql("personajes", "LlaveLab", InfoPersonaje[id][pID], pDroga[playerid][LabID]);
	}else if(Lab[labid][Llave2l] == -1){
		pDroga[id][LlaveLab] = pDroga[playerid][LabID];
		Lab[labid][Llave2l] = InfoPersonaje[id][pID];
		guardar_int_mysql("personajes", "LlaveLab", InfoPersonaje[id][pID], pDroga[playerid][LabID]);
	}else if(Lab[labid][Llave3l] == -1){
		pDroga[id][LlaveLab] = pDroga[playerid][LabID];
		guardar_int_mysql("personajes", "LlaveLab", InfoPersonaje[id][pID], pDroga[playerid][LabID]);
