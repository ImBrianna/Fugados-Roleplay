	return 0;
}

Dialog:ChangeNumber(playerid, response, listitem, inputtext[])
{
	if (!response) return pc_cmd_precios(playerid);
	if (InfoCuenta[playerid][jMonedas] < m_cambiodenumero) return SendClientMessageEx(playerid, COLOR_GREY, "* Necesitas %d monedas para comprar esta opción, tienes %d monedas.", m_cambiodenumero, InfoCuenta[playerid][jMonedas]);
	if (!isnumeric(inputtext)) return SendClientMessage(playerid, COLOR_GREY, "* Sólo números porfavor, vuelve a intentarlo.");
	new numero = strval(inputtext);
	if (numero < 100 || numero > 999999) return SendClientMessage(playerid, COLOR_GREY, "* Los digitos deben estar entre 100 a 999999.");
	if (VerificarNumero(numero))
	{
		SendClientMessage(playerid, 0xEBEBEBFF, "El {60bd7e}número telefónico{EBEBEB} ya está en uso, intenta con otro.");
		Dialog_Show(playerid, ChangeNumber, DIALOG_STYLE_INPUT, "{EBEBEB}Cambio de {60bd7e}número telefónico", "{FFFFFF}Introduce un {00AACC}número{FFFFFF} (entre 100 a 999999).\nEjemplo: {00cc00}555 {FFFFFF}o {00cc00}606060\n", "Continuar", "Volver");
		return 1;
	}
	InfoPersonaje[playerid][pTelefono] = numero;
	guardar_int_mysql("personajes", "Telefono", InfoPersonaje[playerid][pID], InfoPersonaje[playerid][pTelefono]);
	InfoCuenta[playerid][jMonedas] -= m_cambiodenumero;
	guardar_int_mysql("cuentas", "KawaiAwA", InfoCuenta[playerid][jID], InfoCuenta[playerid][jMonedas]);
	SendClientMessageEx(playerid, 0x60bd7eFF, "Cambiaste tu número telefónico al %d por %d monedas, te quedan %d monedas.", numero, m_cambiodenumero, InfoCuenta[playerid][jMonedas]);
	return 1;
}

