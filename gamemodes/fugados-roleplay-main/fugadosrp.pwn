Dialog:Compartir2Negocio(playerid, response, listitem, inputtext[])
{
	if (!response) return NegocioShareShow(playerid);
	new n = EnNegocio(playerid);
	if (n == -1) return SendClientMessage(playerid, COLOR_GREY, "* No estás en ningún negocio.");
	if (LlavesNegocio(playerid, n) == 0) return SendClientMessage(playerid, COLOR_GREY, "* No tienes las llaves de este negocio.");
	if (!isnumeric(inputtext)) return SendClientMessage(playerid, COLOR_GREY, "* Sólo puedes ingresar números, intenta de nuevo.");
	new playerid2 = strval(inputtext);
	if (playerid == playerid2) return SendClientMessage(playerid, COLOR_GREY, "* No puedes compartir permisos del negocio a ti mismo.");
	if (!IsPlayerConnectedEx(playerid2)) return SendClientMessage(playerid, COLOR_GREY, "* El jugador seleccionado no se encuentra conectado.");
	if (strmatch(InfoNegocio[n][nCompartida1], InfoPersonaje[playerid2][pNombre_Apellido])) { SendClientMessage(playerid, COLOR_GREY, "* Ese jugador ya tiene permisos en tu negocio, revisa slot 1."); return 1; }
	if (strmatch(InfoNegocio[n][nCompartida2], InfoPersonaje[playerid2][pNombre_Apellido])) { SendClientMessage(playerid, COLOR_GREY, "* Ese jugador ya tiene permisos en tu negocio, revisa slot 2."); return 1; }
	if (strmatch(InfoNegocio[n][nCompartida3], InfoPersonaje[playerid2][pNombre_Apellido])) { SendClientMessage(playerid, COLOR_GREY, "* Ese jugador ya tiene permisos en tu negocio, revisa slot 3."); return 1; }
	switch (ShareNegocio[playerid])
	{
	    case 1: { alm(InfoNegocio[n][nCompartida1], InfoPersonaje[playerid2][pNombre_Apellido]); guardar_string_mysql("negocios", "Compartida1", InfoNegocio[n][nID_sql], InfoNegocio[n][nCompartida1]); }
	    case 2: { alm(InfoNegocio[n][nCompartida2], InfoPersonaje[playerid2][pNombre_Apellido]); guardar_string_mysql("negocios", "Compartida2", InfoNegocio[n][nID_sql], InfoNegocio[n][nCompartida2]); }
	    case 3: { alm(InfoNegocio[n][nCompartida3], InfoPersonaje[playerid2][pNombre_Apellido]); guardar_string_mysql("negocios", "Compartida3", InfoNegocio[n][nID_sql], InfoNegocio[n][nCompartida3]); }
	}
	SendClientMessageEx(playerid2, 0x59F37BFF, "* %s te ha dado permisos de su negocio %d.", NombreOriginal(playerid), n);
	SendClientMessageEx(playerid, 0x59F37BFF, "* Cediste permisos de tu negocio %d con %s (slot %d).", n, NombreOriginal(playerid2), ShareNegocio[playerid]);
	return 1;
}

