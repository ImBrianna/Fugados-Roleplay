Dialog:ShowOrigen(playerid, response, listitem, inputtext[])
{
	if (!response)
	{
		new gdialog[190];
		format(gdialog, sizeof gdialog, "{FFFFFF}¿Qué edad tendrá {00AACC}%s{FFFFFF}?\n", InfoPersonaje[playerid][pNombre_Apellido]);
		Dialog_Show(playerid, ShowEdad, DIALOG_STYLE_INPUT, "{FFFFFF}Edad", gdialog, "Continuar", "Volver");
		return 1;
	}
	new ciudad[18], gformat[256], gdialog[1028], sexo[16];
	switch (listitem)
	{
		case 0: InfoPersonaje[playerid][pCiudad] = 1, ciudad = "los santos";
		case 1: InfoPersonaje[playerid][pCiudad] = 2, ciudad = "san fierro";
		case 2: InfoPersonaje[playerid][pCiudad] = 3, ciudad = "las venturas";
	}
	if (InfoPersonaje[playerid][pSexo] == 1) sexo = "masculino";
	else sexo = "femenino";
	format(gformat, sizeof gformat, "{FFFFFF}Nombre y apellido: {00AACC}%s\n", InfoPersonaje[playerid][pNombre_Apellido]); strcat(gdialog, gformat);
	format(gformat, sizeof gformat, "{FFFFFF}Edad: {00AACC}%d años\n", InfoPersonaje[playerid][pEdad]); strcat(gdialog, gformat);
	format(gformat, sizeof gformat, "{FFFFFF}Sexo: {00AACC}%s\n", sexo); strcat(gdialog, gformat);
	format(gformat, sizeof gformat, "{FFFFFF}Origen: {00AACC}%s\n\n", ciudad); strcat(gdialog, gformat);
	strcat(gdialog, "{FFFFFF}Sí los datos son correctos presiona '{00CC00}jugar{FFFFFF}' sino en '{FFBF00}volver{FFFFFF}'\n");
	Dialog_Show(playerid, EmpezarPersonaje, DIALOG_STYLE_MSGBOX, "{FFFFFF}Confirmación", gdialog, "Jugar", "Volver");
	return 1;
}

Dialog:EmpezarPersonaje(playerid, response, listitem, inputtext[])
{
	if (!response) return Dialog_Show(playerid, ShowOrigen, DIALOG_STYLE_LIST, "{FFFFFF}Origen de tu personaje", "Los Santos\nSan Fierro\nLas Venturas", "Continuar", "Volver");
	TogglePlayerSpectating(playerid, false);
	StopAudioStreamForPlayer(playerid);
	InfoPersonaje[playerid][pPosicionX] = 1645.7599;
	InfoPersonaje[playerid][pPosicionY] = -1754.4899;
	InfoPersonaje[playerid][pPosicionZ] = 13.5469;
	InfoPersonaje[playerid][pPosicionR] = 260.2141;
	InfoPersonaje[playerid][pInterior] = 0;
	InfoPersonaje[playerid][pVirtualWorld] = 0;
	if (InfoPersonaje[playerid][pSexo] == 1) { InfoPersonaje[playerid][pSkin] = 299, SetPlayerSkin(playerid, 299); }
	else { InfoPersonaje[playerid][pSkin] = 41, SetPlayerSkin(playerid, 41); }
	if (!isnull(InfoPersonaje[playerid][pNombre_Apellido])) SetPlayerName(playerid, InfoPersonaje[playerid][pNombre_Apellido]);
	else { Kick(playerid); return 1; }
	InfoPersonaje[playerid][pVida] = 100;
	SetPlayerHealth(playerid, 100);
	usuario_registro[playerid] = 2;
	ClearChat(playerid);
	SendClientMessageEx(playerid, COLOR_YELLOW, "Hola %s, registraste con éxito en Servercito Roleplay.", NombreJugador(playerid));
	SendClientMessage(playerid, COLOR_YELLOW, "Puedes usar /gps para ubicar lugares de la ciudad, si tienes dudas usa /soporte.");
	SendClientMessage(playerid, 0x6D88F8FF, "Para conseguir un trabajo averigua uno con /empleos, y la sede de cada uno usando /base.");
	SetPlayerScore(playerid, 1);
	InfoPersonaje[playerid][pHorasOn] = 0;
	state_on[playerid] = 3; // logeado
	new DineroInicial = 1250; // Stats de Inicio.
	InfoPersonaje[playerid][pDinero] = DineroInicial;
	GivePlayerMoney(playerid, DineroInicial);
	SetPosEx(playerid, InfoPersonaje[playerid][pPosicionX], InfoPersonaje[playerid][pPosicionY], InfoPersonaje[playerid][pPosicionZ], InfoPersonaje[playerid][pPosicionR], InfoPersonaje[playerid][pInterior], InfoPersonaje[playerid][pVirtualWorld], 0);
	TogglePlayerControllable(playerid, true);
	SetCameraBehindPlayer(playerid);
	SetPlayerColor(playerid, 0xFFFFFF00);
	//ShowModelSelectionMenu(playerid, skinlist, "Selecciona un skin");
	InfoPersonaje[playerid][pFechaRegistro] = FechaHoy();
	InfoPersonaje[playerid][pUltimaConexion] = FechaHoy();
	InfoPersonaje[playerid][pCuentaID] = InfoCuenta[playerid][jID];
	new query[2056];
	mysql_format(db_samp, query, sizeof query, "INSERT INTO personajes (Nombre_Apellido, CuentaID, PosicionX, PosicionY, PosicionZ, PosicionR, Interior, VirtualWorld, Skin, Sexo, Edad, Ciudad, BolosUwU, FechaRegistro, UltimaConexion) \
	VALUES ('%e', '%d', '%f', '%f', '%f', '%f', '%d', '%d', '%d', '%d', '%d', '%d', '%d', '%e', '%e')",
	NombreOriginal(playerid),
	InfoPersonaje[playerid][pCuentaID],
	InfoPersonaje[playerid][pPosicionX],
	InfoPersonaje[playerid][pPosicionY],
	InfoPersonaje[playerid][pPosicionZ],
	InfoPersonaje[playerid][pPosicionR],
	InfoPersonaje[playerid][pInterior],
	InfoPersonaje[playerid][pVirtualWorld],
	InfoPersonaje[playerid][pSkin],
	InfoPersonaje[playerid][pSexo],
	InfoPersonaje[playerid][pEdad],
	InfoPersonaje[playerid][pCiudad],
	DineroInicial,
	InfoPersonaje[playerid][pFechaRegistro],
	InfoPersonaje[playerid][pUltimaConexion]);
	mysql_tquery(db_samp, query, "CreatePlayerCharacter", "i", playerid);
	return 1;
}

forward CreatePlayerCharacter(playerid);
public CreatePlayerCharacter(playerid)
{
	InfoPersonaje[playerid][pID] = cache_insert_id();
	cache_unset_active();
	InfoPersonaje[playerid][pDocumento] = 2000000+InfoPersonaje[playerid][pID];
	InfoPersonaje[playerid][pTelefono] = 500000+InfoPersonaje[playerid][pID];
	new query[512];
	mysql_format(db_samp, query, sizeof query, "UPDATE personajes SET Telefono = '%d', Documento = '%d', Mano_izquierda = 50, Cantidad_mano_izquierda = 1 WHERE ID = '%d'", InfoPersonaje[playerid][pTelefono], InfoPersonaje[playerid][pDocumento], InfoPersonaje[playerid][pID]);
	mysql_tquery(db_samp, query);
	PlayerTextDrawShow(playerid, TDping[playerid]);
	InfoPersonaje[playerid][pManoIzquierda] = 50;
	InfoPersonaje[playerid][pManoIzquierdaCant] = 1;
	PonerObjeto(playerid, 2, 50);
	return 1;
}

