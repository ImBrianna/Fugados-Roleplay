cmd:pagar(playerid, params[])
{
	if (InfoPersonaje[playerid][pHorasOn] < 5) return SendClientMessage(playerid, COLOR_GREY, "* Necesitas jugar al menos 5 horas el servidor.");
	new string[256];
	if (sscanf(params, "dd", params[0], params[1])) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /pagar (JugadorID) (Cantidad $)");
	if (!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_GREY, "Jugador desconectado.");
	if (playerid == params[0]) return SendClientMessage(playerid, COLOR_GREY, "Buenooo.. tiras el dinero arriba y te cae encimaXd??");
	if (params[1] > GetPlayerCash(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No posees esa cantidad.");
	if (params[1] < 1 || params[1] > 15000) return SendClientMessage(playerid, COLOR_GREY, "Sólo puedes pasar entre 1 y 15,000 dólares.");
	if (!CercaDelMarica(3.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Jugador muy lejos.");
	GivePlayerCash(playerid, -params[1], "pagar");
	GivePlayerCash(params[0], params[1], "pagar");
	new Float:X, Float:Y, Float:Z;
	GetPlayerPos(playerid, X, Y, Z);
	if (InfoCuenta[playerid][jMulticuenta] == 0 && InfoCuenta[params[0]][jMulticuenta] == 0)
	{
		if (!strcmp(InfoCuenta[playerid][jIP], InfoCuenta[params[0]][jIP], false))
		{
			new ipc[32], stringlog[180]; GetPlayerIp(playerid, ipc, 32);
			format(stringlog, sizeof(stringlog), "[ATENCIÓN] Posibles multicuentas: %s(%d) ha pagado [$%d] a %s(%d). [IP: %s]", NombreOriginal(playerid), playerid, params[1],NombreOriginal(params[0]), params[0], ipc);
			SendAdminMessage(COLOR_ORANGE, stringlog); Log("f_logs/multicuentas.log", stringlog);
		}
	}
	PlayerPlaySound(playerid, 1052, X, Y, Z);
	PlayerPlaySound(params[0], 1052, X, Y, Z);
	format(string, sizeof string, "saca $%s dólares y se los da %s.", diner_coma(params[1]), NombreJugador(params[0]));
	rol_auto(playerid, string);
	format(string, sizeof string, "* %s pagó %d$ a %s", NombreDeCuenta(playerid), params[1], NombreDeCuenta(params[0]));
	Log("f_logs/pagos.log", string);
	return 1;
}

