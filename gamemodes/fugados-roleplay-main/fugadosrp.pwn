cmd:crearincendio(playerid, params[])
{
	if (CheckAdminCMD(playerid, 3)) return 1;
	if (CheckAdminDuty(playerid)) return 1;
	new tipo, id_propiedad, nextFire = FuegoSiguiente(), gformat[256];
	if (nextFire == -1) return SendClientMessage(playerid, COLOR_GREY, "* No se pueden crear más incendios, se llegó al limite de 20.");
	if (sscanf(params, "dd", tipo, id_propiedad)) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /crearincendio [tipo propiedad (0casa 1negocio)] [id propiedad]");
	if (tipo < 0 || tipo > 1) return SendClientMessage(playerid, COLOR_GREY, "* Sólo existen dos tipos (0 casa) (1 negocio)");
	if (tipo == 0)
	{
		if (id_propiedad < 1 || id_propiedad > totalcasas) return SendClientMessage(playerid, COLOR_GREY, "Casa no encontrada.");
		if (InfoCasa[id_propiedad][cTipo] == 0) return SendClientMessage(playerid, COLOR_GREY, "Esa casa no existe.");
		format(gformat, sizeof gformat, "{FF2F2F}[AdmCmd]{FFFFFF} %s(%d) creó un incendio en la casa %d.", InfoCuenta[playerid][jNombre], playerid, id_propiedad);
		SendAdminMessage(0xFFFFFFFF, gformat);
		CreateFireGeneral(id_propiedad, tipo, 0);
	}
	else if (tipo == 1)
	{
		if (id_propiedad < 1 || id_propiedad >= totalnegocios) return SendClientMessage(playerid, -1, "ID de negocio inválida.");
		if (InfoNegocio[id_propiedad][nTipo] == 0) return SendClientMessage(playerid, -1, "Esa ID está vacía.");
		format(gformat, sizeof gformat, "{FF2F2F}[AdmCmd]{FFFFFF} %s(%d) creó un incendio en el negocio %d.", InfoCuenta[playerid][jNombre], playerid, id_propiedad);
		SendAdminMessage(0xFFFFFFFF, gformat);
		CreateFireGeneral(id_propiedad, tipo, 0);
	}
	return 1;
}

