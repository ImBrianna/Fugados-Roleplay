funcion SueloTime(i)
{
	if (IsValidDynamicObject(ObjetoCaido[i])) DestroyDynamicObject(ObjetoCaido[i]);
	ObjetoCaido[i] = 0;
	if (IsValidDynamic3DTextLabel(InfoSuelo[i][DropLabel]))
	{
		DestroyDynamic3DTextLabel(InfoSuelo[i][DropLabel]);
		InfoSuelo[i][DropLabel] = Text3D:INVALID_3DTEXT_ID;
	}
	InfoSuelo[i][DropLabel] = CreateDynamic3DTextLabel("dropid", -1, -1178.1569, 58.9069, 1.9609, 1.0, .worldid = 1+777, .interiorid = i);
	InfoSuelo[i][DropPosX] = 0.0;
	InfoSuelo[i][DropPosY] = 0.0;
	InfoSuelo[i][DropPosZ] = 0.0;
	InfoSuelo[i][DropID] = 0;
	InfoSuelo[i][DropCantidad] = 0;
	KillTimer(ObjTimer[i]);
	return 1;
}

//info caja
enum j_info
{
	cjID,
	cjActivo,
	cjObjeto[10],
	cjObjetoCant[10],
};
new InfoCaja[MAX_CAJAS][j_info];

crear_caja(playerid)
{
	Loop(id, MAX_CAJAS, 1) if (InfoCaja[id][cjActivo] == 0)
	{
	    InfoCaja[id][cjActivo] = 1;
		new query[128];
		mysql_format(db_samp, query, sizeof query, "INSERT INTO cajas (Activo) VALUES ('%d')", InfoCaja[id][cjActivo]);
		mysql_tquery(db_samp, query, "CreatePlayerCaja", "ii", playerid, id);
		id = MAX_CAJAS;
	}
	return 1;
}

forward CreatePlayerCaja(playerid, id);
public CreatePlayerCaja(playerid, id)
{
	InfoCaja[id][cjID] = cache_insert_id();
	cache_unset_active();
	InfoPersonaje[playerid][pManoDerecha] = 55;
	InfoPersonaje[playerid][pManoDerechaCant] = InfoCaja[id][cjID];
	PonerObjeto(playerid, 1, 55);
	new query[256];
	mysql_format(db_samp, query, sizeof query, "UPDATE personajes SET Mano_derecha = 55, Cantidad_mano_derecha = '%d' WHERE ID = '%d'", InfoPersonaje[playerid][pManoDerechaCant], InfoPersonaje[playerid][pID]);
	mysql_tquery(db_samp, query);
	totalcajas++;
	return 1;
}

forward CargarCajas();
public CargarCajas()
{
	new cargaditos = 0;
	Loop(tabla, cache_num_rows(), 0)
	{
	    new id = tabla+1;
		rebel_int(tabla, "ID", InfoCaja[id][cjID]);
		rebel_int(tabla, "Activo", InfoCaja[id][cjActivo]);
		rebel_int(tabla, "Obj1", InfoCaja[id][cjObjeto][0]);
		rebel_int(tabla, "ObjCant1", InfoCaja[id][cjObjetoCant][0]);
		rebel_int(tabla, "Obj2", InfoCaja[id][cjObjeto][1]);
		rebel_int(tabla, "ObjCant2", InfoCaja[id][cjObjetoCant][1]);
