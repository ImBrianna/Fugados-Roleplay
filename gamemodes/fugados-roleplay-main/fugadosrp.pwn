CheckPropiedades()
{
	new countH = 0, countB = 0, gformat[320];
	//casas
	Loop(id, totalcasas, 1) if (InfoCasa[id][cTipo] != 0 && InfoCasa[id][cVenta] != 0 && InfoCasa[id][cPremium] == 0)
	{
		if (InfoCasa[id][cTiempoDisponible] < gettime())
