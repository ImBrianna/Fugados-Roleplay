cmd:id(playerid, params[])
{
	new id, string[232], colorping[19];
	if (isnull(params)) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /id (Nombre o ID)");
	if (isnumeric(params))
	{
		sscanf(params, "d", id);
		if (!IsPlayerConnected(id)) return SendClientMessage(playerid, COLOR_GREY, "Jugador desconectado.");
		if (InfoPersonaje[id][pHorasOn] == -1) return SendClientMessage(playerid, COLOR_GREY, "Ese jugador está logeando.");
		switch (GetPlayerPing(id))
		{
			case 0..150: alm(colorping, "{46e800}");
			case 151..200: alm(colorping, "{E0F600}");
			case 201..250: alm(colorping, "{FED32C}");
			case 251..300: alm(colorping, "{f7e300}");
			default: alm(colorping, "{f74000}");
		}
		if (InfoCuenta[playerid][jAdmin] != 0) format(string, sizeof string, "[ID: %d] {00AACC}%s{C4C4FF} (Horas: %d) (Ping: %s%d{C4C4FF}) (Cuenta: {FFBF00}%s{C4C4FF})", id, Nombre2Original(id),  InfoPersonaje[id][pHorasOn], colorping, GetPlayerPing(id), InfoCuenta[id][jNombre]);
		else format(string, sizeof string, "[ID: %d] {00AACC}%s{C4C4FF} (Horas: %d) (Ping: %s%d{C4C4FF})", id, Nombre2Original(id),  InfoPersonaje[id][pHorasOn], colorping, GetPlayerPing(id));
		SendClientMessage(playerid, 0xC4C4FFFF, string);
	}
	else
	{
		if (strlen(params) < 3 || strlen(params) > MAX_PLAYER_NAME) return SendClientMessage(playerid, COLOR_GREY, "El nombre del jugador debe estar entre 3 a 24 caracteres.");
		new count;
		foreach (new playerid2: Player) if (strfind(ReturnName(playerid2), params, true) != -1)
		{
			if (InfoPersonaje[playerid2][pHorasOn] != -1)
			{
				switch (GetPlayerPing(playerid2))
				{
					case 0..150: alm(colorping, "{46e800}");
					case 151..200: alm(colorping, "{E0F600}");
					case 201..250: alm(colorping, "{FED32C}");
					case 251..300: alm(colorping, "{f7e300}");
					default: alm(colorping, "{f74000}");
				}
				if (InfoCuenta[playerid][jAdmin] != 0) format(string, sizeof string, "[ID: %d] {00AACC}%s{C4C4FF} (Horas: %d) (Ping: %s%d{C4C4FF}) (Cuenta: {FFBF00}%s{C4C4FF})", playerid2, Nombre2Original(playerid2),  InfoPersonaje[playerid2][pHorasOn], colorping, GetPlayerPing(playerid2), InfoCuenta[playerid2][jNombre]);
				else format(string, sizeof string, "[ID: %d] {00AACC}%s{C4C4FF} (Horas: %d) (Ping: %s%d{C4C4FF})", playerid2, Nombre2Original(playerid2),  InfoPersonaje[playerid2][pHorasOn], colorping, GetPlayerPing(playerid2));
				SendClientMessage(playerid, 0xC4C4FFFF, string);
				count++;
			}
		}
		if (!count) return SendClientMessage(playerid, COLOR_GREY, "No hay ningún jugador con ese nombre conectado.");
	}
	return 1;
}

