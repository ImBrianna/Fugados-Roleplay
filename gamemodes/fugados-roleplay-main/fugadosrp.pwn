Dialog:GirarMueble(playerid, response, listitem, inputtext[])
{
	new id = is_mueble_actual[playerid], query[512];
	if (check_id_prop_mueble(playerid) == -1) return pc_cmd_muebles(playerid);
	if (id == 0) return SendClientMessage(playerid, COLOR_GREY, "* Has seleccionado un slot sin mueble.");
	if (InfoMueble[id][mVirtualWorld] == 0) return SendClientMessage(playerid, COLOR_GREY, "* Has seleccionado un slot sin mueble.");
	if (!response) return Dialogo_Editar_Mueble(playerid, id);
	if (InfoMueble[id][mInterior] == -1) return SendClientMessage(playerid, COLOR_GREY, "* Ese mueble está oculto o no lo has agregado.");
	new Float: grados = 0.0;
	switch (listitem)
	{
		case 0:
		{
		    if (InfoMueble[id][mRotZ] == 0.0) return SendClientMessage(playerid, COLOR_GREY, "* El mueble ya está en 0 grados.");
			grados = 0.0;
		}
		case 1:
		{
		    if (InfoMueble[id][mRotZ] == 90.0) return SendClientMessage(playerid, COLOR_GREY, "* El mueble ya está en 90 grados.");
			grados = 90.0;
		}
		case 2:
		{
		    if (InfoMueble[id][mRotZ] == 180.0) return SendClientMessage(playerid, COLOR_GREY, "* El mueble ya está en 180 grados.");
			grados = 180.0;
		}
		case 3:
		{
		    if (InfoMueble[id][mRotZ] == 270.0) return SendClientMessage(playerid, COLOR_GREY, "* El mueble ya está en 270 grados.");
			grados = 270.0;
		}
	}
	format(query, sizeof query, "Giraste %s %0.f grados.", InfoMueble[id][mNombre], grados);
	SendClientMessage(playerid, COLOR_GREY, query);
	GetDynamicObjectRot(InfoMueble[id][mObjeto], InfoMueble[id][mRotX], InfoMueble[id][mRotY], InfoMueble[id][mRotZ]);
	InfoMueble[id][mRotZ] = grados;
	SetDynamicObjectRot(InfoMueble[id][mObjeto], InfoMueble[id][mRotX], InfoMueble[id][mRotY], InfoMueble[id][mRotZ]);
	mysql_format(db_samp, query, sizeof query, "UPDATE muebles SET RotX = '%f', RotY = '%f', RotZ = '%f' WHERE ID = '%d'", InfoMueble[id][mRotX], InfoMueble[id][mRotY], InfoMueble[id][mRotZ], InfoMueble[id][mID_sql]);
	mysql_tquery(db_samp, query);
	return 1;
}

