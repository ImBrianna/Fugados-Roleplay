	ShowVehColor(playerid, "", 2);
	return 1;
}

Dialog:Change2ColorVeh(playerid, response, listitem, inputtext[])
{
	if (NoEstaEnTaller(playerid) == 1) return 1;
	if (!response) return ShowVehColor(playerid, "", 1); //color
