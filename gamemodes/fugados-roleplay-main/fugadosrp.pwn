forward OnShowCharacters(playerid);
public OnShowCharacters(playerid)
{
	new query[210];
	format(query, sizeof query, "{FFFFFF}Personajes de {00AACC}%s", InfoCuenta[playerid][jNombre]);
	if (cache_num_rows() > 0)
	{
		new gformat[320], dialog[2056] = "Personaje\tÚltima conexión\n", count = 0;
		Loop(id, cache_num_rows(), 0)
		{
			new pDI, pName[MAX_PLAYER_NAME], pConexion[256];
			rebel_int(id, "ID", pDI);
			rebel_string(id, "Nombre_Apellido", pName, MAX_PLAYER_NAME);
			rebel_string(id, "UltimaConexion", pConexion, 256);
			//dialogo string
			format(gformat, sizeof gformat, "{FFFFFF}%s\t{E24D32}%s\n", pName, pConexion);
			strcat(dialog, gformat);
			//selector personaje
			format(gformat, sizeof gformat, "id_rows%d", count);
			SetPVarInt(playerid, gformat, pDI);
			count++;
		}
		SetPVarInt(playerid, "max_pjs", count);
		if (InfoCuenta[playerid][jRanuras] != count) strcat(dialog, "{FFBF00}Crear nuevo personaje");
		Dialog_Show(playerid, SeleccionarPersonaje, DIALOG_STYLE_TABLIST_HEADERS, query, dialog, "Continuar", "Salir");
	}
	else
	{
		new dialogito[1028];
		strcat(dialogito, "{FFFFFF}Nuestro nuevo {FFBF00}sistema de cuentas{FFFFFF} te permite tener múltiples personajes en una sola.\n\n");
		strcat(dialogito, "{FFFFFF}El máximo de personajes actualmente de tu cuenta es de {00AACC}3 ranuras{FFFFFF}, si deseas una extra\n");
		strcat(dialogito, "{FFFFFF}contacta un admin encargado de ventas, para aumentar tu máximo de personajes.\n\n");
		strcat(dialogito, "{FFFFFF}Estás a punto de crear un personaje, introduce el nombre del mismo.\nEs importante usar el formato {00AACC}Nombre_Apellido{FFFFFF} (Ejemplo: {00CC00}Camila_Rodriguez{FFFFFF})\n");
		Dialog_Show(playerid, CrearPersonaje, DIALOG_STYLE_INPUT, "{FFFFFF}Creación de personaje", dialogito, "Continuar", "Salir");
		SetPVarInt(playerid, "QhacerPj", 1);
		SetPVarInt(playerid, "max_pjs", 1);
	}
	return 1;
}

Dialog:CrearPersonaje(playerid, response, listitem, inputtext[])
{
	if (!response)
	{
		if (GetPVarInt(playerid, "max_pjs") == 1) Kick(playerid);
		else VerPersonajes(playerid);
		return 1;
	}
	if (isnull(inputtext) || strlen(inputtext) > 23)
	{
		SendClientMessage(playerid, 0xEBEBEBFF, "El {00AACC}Nombre_Apellido{EBEBEB} no puede ser superior a 24 carácteres.");
		Dialog_Show(playerid, CrearPersonaje, DIALOG_STYLE_INPUT, "{FFFFFF}Creación de personaje", "{FFFFFF}Estás a punto de crear un personaje, introduce el nombre del mismo.\nEs importante usar el formato {00AACC}Nombre_Apellido{FFFFFF} (Ejemplo: {00CC00}Camila_Rodriguez{FFFFFF})\n", "Continuar", "Volver");
		return 1;
	}
	if (!IsValidRoleplayName(inputtext))
	{
