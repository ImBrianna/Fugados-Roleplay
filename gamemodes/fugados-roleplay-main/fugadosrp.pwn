cmd:renunciar(playerid, params[])
{
	new string[256];
	if (isnull(params))
	{
		if (InfoPersonaje[playerid][pTrabajo1] != -1)
		{
			format(string, sizeof string, "Para dejar el empleo {00AACC}%s{FFFFFF}, escribe {00AACC}/dejarempleo 1{FFFFFF}.", GetNameContrato(InfoPersonaje[playerid][pTrabajo1]));
			SendClientMessage(playerid, 0xFFFFFFFF, string);
		}
		if (InfoPersonaje[playerid][pTrabajo2] != -1)
		{
			format(string, sizeof string, "Para dejar el empleo {00AACC}%s{FFFFFF}, escribe {00AACC}/dejarempleo 2{FFFFFF}.", GetNameContrato(InfoPersonaje[playerid][pTrabajo2]));
			SendClientMessage(playerid, 0xFFFFFFFF, string);
		}
		if (InfoPersonaje[playerid][pTrabajo3] != -1)
		{
			format(string, sizeof string, "Para dejar el empleo {00AACC}%s{FFFFFF}, escribe {00AACC}/dejarempleo 3{FFFFFF}.", GetNameContrato(InfoPersonaje[playerid][pTrabajo3]));
			SendClientMessage(playerid, 0xFFFFFFFF, string);
		}
	}
	else
	{
		new id_job;
		if (sscanf(params, "d", id_job)) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /dejarempleo (Slot)");
		switch (id_job)
		{
			case 1:
			{
				if (InfoPersonaje[playerid][pTrabajo1] == -1) return SendClientMessage(playerid, COLOR_GREY, "* No tienes ningún empleo en el slot 1.");
				format(string, sizeof string, "* Dejaste tu puesto de %s.", GetNameContrato(InfoPersonaje[playerid][pTrabajo1]));
				SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
				InfoPersonaje[playerid][pTrabajo1] = -1;
				guardar_int_mysql("personajes", "Trabajo1", InfoPersonaje[playerid][pID], -1);
			}
			case 2:
			{
				if (InfoPersonaje[playerid][pTrabajo2] == -1) return SendClientMessage(playerid, COLOR_GREY, "* No tienes ningún empleo en el slot 2.");
				format(string, sizeof string, "* Dejaste tu puesto de %s.", GetNameContrato(InfoPersonaje[playerid][pTrabajo2]));
				SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
				InfoPersonaje[playerid][pTrabajo2] = -1;
				guardar_int_mysql("personajes", "Trabajo2", InfoPersonaje[playerid][pID], -1);
			}
			case 3:
			{
				if (InfoPersonaje[playerid][pTrabajo3] == -1) return SendClientMessage(playerid, COLOR_GREY, "* No tienes ningún empleo en el slot 3.");
				format(string, sizeof string, "* Dejaste tu puesto de %s.", GetNameContrato(InfoPersonaje[playerid][pTrabajo3]));
				SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
				InfoPersonaje[playerid][pTrabajo3] = -1;
				guardar_int_mysql("personajes", "Trabajo3", InfoPersonaje[playerid][pID], -1);
			}
			default: SendClientMessage(playerid, COLOR_GREY, "* Sólo hay dos slots de empleo. (ejemplo: /dejarempleo 3)");
		}
	}
	return 1;
}
