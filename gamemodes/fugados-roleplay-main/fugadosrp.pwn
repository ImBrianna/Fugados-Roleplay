ObtenerLaboratorio(playerid){
	Loop(p, MAX_LABORATORIOS, 1) if(Lab[p][EnUso])
	{
		if((IsPlayerInRangeOfPoint(playerid, 2.0, Lab[p][Entrada][0], Lab[p][Entrada][1], Lab[p][Entrada][2]) && GetPlayerVirtualWorld(playerid) == 0) ||
		GetPlayerVirtualWorld(playerid) == 277+Lab[p][sID]) return p;
