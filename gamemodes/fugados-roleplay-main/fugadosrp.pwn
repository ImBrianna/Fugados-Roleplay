AdminOnlineCheck()
{
	new count;
	foreach(Player, playerid) if (IsPlayerConnectedEx(playerid))
	{
		if (GetPlayerAdmin(playerid, 1) != 0)
		{
			count++;
		}
	}
	return count;
}

SendAdminMessage(color, gformat[])
{
	foreach(Player, playerid) if (IsPlayerConnectedEx(playerid))
	{
		if (GetPlayerAdmin(playerid, 1) == 1 && AdminChat[playerid] == 0)
		{
			SendSplitMessage(playerid, color, gformat);
		}
	}
	return 1;
}

CheckAdminCMD(playerid, rango)
{
	if (!IsPlayerConnectedEx(playerid)) return 1;
	new gformat[190];
	if (GetPlayerAdmin(playerid, rango) == 0)
	{
		new name[39] = "Admin";
		switch (rango)
		{
			case 1: alm(name, "Soporte");
			case 2: alm(name, "Moderador");
			case 3: alm(name, "Administrador");
			case 4: alm(name, "Director");
			case 10: alm(name, "Propietario");
		}
		format(gformat, sizeof gformat, "* Para hacer esto debes ser mínimo rango %s.", name);
		SendClientMessage(playerid, COLOR_GREY, gformat);
		return 1;
	}
	return 0;
}

CheckAdminDuty(playerid)
{
	if (!IsPlayerConnectedEx(playerid)) return 1;
	if (!strcmp(InfoCuenta[playerid][jNombre], "Prieto", true) || !strcmp(InfoCuenta[playerid][jNombre], "Vestige", true) || !strcmp(InfoCuenta[playerid][jNombre], "Elias01", true)) { return 0; }
	if (papilla[playerid] == 0) { SendClientMessage(playerid, COLOR_GREY, "* Para hacer esto debes estar admin duty. (/onstaff)"); return 1; }
	return 0;
}

GetPlayerAdmin(playerid, nivel)
{
	if (!IsPlayerConnectedEx(playerid)) return 0;
	if (nivel == 10)
	{
		if (!strcmp(InfoCuenta[playerid][jNombre], "Prieto", true) || !strcmp(InfoCuenta[playerid][jNombre], "Vestige", true) || !strcmp(InfoCuenta[playerid][jNombre], "Elias01", true)) { return 1; }
	}
	else
	{
		if (!strcmp(InfoCuenta[playerid][jNombre], "Prieto", true) || !strcmp(InfoCuenta[playerid][jNombre], "Vestige", true) || !strcmp(InfoCuenta[playerid][jNombre], "Elias01", true)) { return 1; }
		if (InfoCuenta[playerid][jAdmin] >= nivel) return 1;
	}
	return 0;
}

GetAdminName(playerid)
{
	new name[39] = "Admin";
	if (!strcmp(InfoCuenta[playerid][jNombre], "Prieto", true))
	{
		alm(name, "Propietario");
		return name;
	}
	if (!strcmp(InfoCuenta[playerid][jNombre], "Elias01", true))
	{
		alm(name, "Desarrollador");
		return name;
	}
	if (!strcmp(InfoCuenta[playerid][jNombre], "Vestige", true))
	{
		alm(name, "Desarrollador");
		return name;
	}
	switch (InfoCuenta[playerid][jAdmin])
	{
		case 1: alm(name, "Soporte");
		case 2: alm(name, "Moderador");
		case 3: alm(name, "Administrador");
		case 4: alm(name, "Director");
	}
	return name;
}

