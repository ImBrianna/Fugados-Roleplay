ApplyAnimEx(playerid, vehicle, animlib[],animname[], Float:Speed, looping, lockx, locky, lockz, lp)
{
	if (GetPVarInt(playerid, "EstaCrack") == 1) return SendClientMessage(playerid, COLOR_GREY, "No puedes usar animaciones mientras estás herido.");
 	if (GetPVarInt(playerid, "PlayerCuffed") != 0) return SendClientMessage(playerid, COLOR_GREY, "No puedes usar animaciones en ese estado.");
	if (InfoPersonaje[playerid][pModoTalar] != -1) return SendClientMessage(playerid, COLOR_GREY, "No puedes usar animaciones mientras trabajas de aserrador.");
	if (vehicle == 0) { if (IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "No puedes usar animaciones mientras estés dentro de un vehículo."); }
	if (GetPVarInt(playerid, "IsFrozen") == 1) return SendClientMessage(playerid, COLOR_GREY, "No puedes usar animaciones porque estás congelado.");
	gPlayerUsingLoopingAnim[playerid] = 1;
	ApplyAnimation(playerid, animlib, animname, Speed, looping, lockx, locky, lockz, lp, 1);
