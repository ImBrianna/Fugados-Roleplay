forward CargarMuebles();
public CargarMuebles()
{
	new cargaditos = 0, decasas = 0, denegocios = 0, texturas = 0;
	Loop(tabla, cache_num_rows(), 0)
	{
	    new id = tabla+1;

		rebel_int(tabla, "ID", InfoMueble[id][mID_sql]);
		rebel_int(tabla, "Tipo", InfoMueble[id][mTipo]);
		rebel_int(tabla, "Propiedad", InfoMueble[id][mPropiedad]);
		rebel_int(tabla, "Modelo", InfoMueble[id][mModelo]);
		rebel_string(tabla, "Nombre", InfoMueble[id][mNombre], 128);
		rebel_float(tabla, "PotX", InfoMueble[id][mPotX]);
		rebel_float(tabla, "PotY", InfoMueble[id][mPotY]);
		rebel_float(tabla, "PotZ", InfoMueble[id][mPotZ]);
		rebel_float(tabla, "RotX", InfoMueble[id][mRotX]);
		rebel_float(tabla, "RotY", InfoMueble[id][mRotY]);
		rebel_float(tabla, "RotZ", InfoMueble[id][mRotZ]);
		rebel_int(tabla, "Interior", InfoMueble[id][mInterior]);
		rebel_int(tabla, "VirtualWorld", InfoMueble[id][mVirtualWorld]);
		rebel_int(tabla, "Precio", InfoMueble[id][mPrecio]);
		rebel_int(tabla, "Puerta", InfoMueble[id][mPuerta]);
		rebel_int(tabla, "TxtMaterial1", InfoMueble[id][mTxtMaterial1]);
		rebel_int(tabla, "TxtModelo1", InfoMueble[id][mTxtModelo1]);
		rebel_string(tabla, "TxtNombre1", InfoMueble[id][mTxtNombre1], 128);
		rebel_string(tabla, "TxtNombreTextura1", InfoMueble[id][mTxtNombreTextura1], 128);
		rebel_int(tabla, "TxtColor1", InfoMueble[id][mTxtColor1]);
		rebel_int(tabla, "TxtMaterial2", InfoMueble[id][mTxtMaterial2]);
		rebel_int(tabla, "TxtModelo2", InfoMueble[id][mTxtModelo2]);
		rebel_string(tabla, "TxtNombre2", InfoMueble[id][mTxtNombre2], 128);
		rebel_string(tabla, "TxtNombreTextura2", InfoMueble[id][mTxtNombreTextura2], 128);
		rebel_int(tabla, "TxtColor2", InfoMueble[id][mTxtColor2]);
		rebel_int(tabla, "TxtMaterial3", InfoMueble[id][mTxtMaterial3]);
		rebel_int(tabla, "TxtModelo3", InfoMueble[id][mTxtModelo3]);
		rebel_string(tabla, "TxtNombre3", InfoMueble[id][mTxtNombre3], 128);
		rebel_string(tabla, "TxtNombreTextura3", InfoMueble[id][mTxtNombreTextura3], 128);
		rebel_int(tabla, "TxtColor3", InfoMueble[id][mTxtColor3]);
		rebel_int(tabla, "TxtMaterial4", InfoMueble[id][mTxtMaterial4]);
		rebel_int(tabla, "TxtModelo4", InfoMueble[id][mTxtModelo4]);
		rebel_string(tabla, "TxtNombre4", InfoMueble[id][mTxtNombre4], 128);
		rebel_string(tabla, "TxtNombreTextura4", InfoMueble[id][mTxtNombreTextura4], 128);
		rebel_int(tabla, "TxtColor4", InfoMueble[id][mTxtColor4]);
		rebel_int(tabla, "TxtMaterial5", InfoMueble[id][mTxtMaterial5]);
		rebel_int(tabla, "TxtModelo5", InfoMueble[id][mTxtModelo5]);
		rebel_string(tabla, "TxtNombre5", InfoMueble[id][mTxtNombre5], 128);
		rebel_string(tabla, "TxtNombreTextura5", InfoMueble[id][mTxtNombreTextura5], 128);
		rebel_int(tabla, "TxtColor5", InfoMueble[id][mTxtColor5]);

		if (InfoMueble[id][mID_sql]) cargaditos++;
		//función
		if (InfoMueble[id][mModelo] != 0)
		{
			switch (InfoMueble[id][mTipo])
			{
				case 1: decasas++;
				case 2: denegocios++;
			}
			totalmuebles++;
		}
		if (InfoMueble[id][mPotX] != 0.0)
		{
			InfoMueble[id][mObjeto] = CreateDynamicObject(InfoMueble[id][mModelo], InfoMueble[id][mPotX], InfoMueble[id][mPotY], InfoMueble[id][mPotZ], InfoMueble[id][mRotX], InfoMueble[id][mRotY], InfoMueble[id][mRotZ], InfoMueble[id][mVirtualWorld], InfoMueble[id][mInterior]);

			if (InfoMueble[id][mTxtModelo1] != 0) { SetDynamicObjectMaterial(InfoMueble[id][mObjeto], InfoMueble[id][mTxtMaterial1], InfoMueble[id][mTxtModelo1], InfoMueble[id][mTxtNombre1], InfoMueble[id][mTxtNombreTextura1], InfoMueble[id][mTxtColor1]); texturas++; }
			if (InfoMueble[id][mTxtModelo2] != 0) { SetDynamicObjectMaterial(InfoMueble[id][mObjeto], InfoMueble[id][mTxtMaterial2], InfoMueble[id][mTxtModelo2], InfoMueble[id][mTxtNombre2], InfoMueble[id][mTxtNombreTextura2], InfoMueble[id][mTxtColor2]); texturas++; }
			if (InfoMueble[id][mTxtModelo3] != 0) { SetDynamicObjectMaterial(InfoMueble[id][mObjeto], InfoMueble[id][mTxtMaterial3], InfoMueble[id][mTxtModelo3], InfoMueble[id][mTxtNombre3], InfoMueble[id][mTxtNombreTextura3], InfoMueble[id][mTxtColor3]); texturas++; }
			if (InfoMueble[id][mTxtModelo4] != 0) { SetDynamicObjectMaterial(InfoMueble[id][mObjeto], InfoMueble[id][mTxtMaterial4], InfoMueble[id][mTxtModelo4], InfoMueble[id][mTxtNombre4], InfoMueble[id][mTxtNombreTextura4], InfoMueble[id][mTxtColor4]); texturas++; }
			if (InfoMueble[id][mTxtModelo5] != 0) { SetDynamicObjectMaterial(InfoMueble[id][mObjeto], InfoMueble[id][mTxtMaterial5], InfoMueble[id][mTxtModelo5], InfoMueble[id][mTxtNombre5], InfoMueble[id][mTxtNombreTextura5], InfoMueble[id][mTxtColor5]); texturas++; }
		}
	}
	printf("%d muebles [c: %d] [n: %d] (SQL %d) cargados correctamente.", totalmuebles-1, decasas, denegocios, cargaditos);
	printf("%d texturas (SQL %d) cargadas correctamente.", texturas, cargaditos);
	CheckPropiedades();
	return 1;
}

