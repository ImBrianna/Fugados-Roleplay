	Planta[id][sID]);
	mysql_tquery(db_samp, query);
}

cmd:plantar(playerid, params[]){
	new manoder = InfoPersonaje[playerid][pManoDerecha];
	new manoizq = InfoPersonaje[playerid][pManoIzquierda];
	if(isnull(params)) return 1;

	if (strcmp(params, "marihuana", true) == 0){
		if(manoder == 83){
			InfoPersonaje[playerid][pManoDerechaCant]--;
			CrearPlanta(playerid, 1);
			return 1;
		}else if(manoizq == 83){
			InfoPersonaje[playerid][pManoIzquierdaCant]--;
			CrearPlanta(playerid, 1);
			return 1;
		}
		return SendClientMessage(playerid, COLOR_GREY, "No tienes semillas de marihuana en tus manos");
	}else if(strcmp(params, "micelio", true) == 0){
		if(manoder == 84){
			InfoPersonaje[playerid][pManoDerechaCant]--;
			CrearPlanta(playerid, 2);
			return 1;
		}else if(manoizq == 84){
			InfoPersonaje[playerid][pManoIzquierdaCant]--;
			CrearPlanta(playerid, 2);
			return 1;
		}
		return SendClientMessage(playerid, COLOR_GREY, "No tienes micelio en tus manos");
	}else if(strcmp(params, "coca", true) == 0){
		if(manoder == 85){
			InfoPersonaje[playerid][pManoDerechaCant]--;
			CrearPlanta(playerid, 3);
			return 1;
		}else if(manoizq == 85){
			InfoPersonaje[playerid][pManoIzquierdaCant]--;
			CrearPlanta(playerid, 3);
			return 1;
		}
		return SendClientMessage(playerid, COLOR_GREY, "No tienes semillas de coca en tus manos");
	}
	SendClientMessage(playerid, COLOR_GREY, "* /plantar (marihuana-micelio-coca)");
	return 1;
}

