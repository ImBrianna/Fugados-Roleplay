	switch (listitem)
	{
		case 0: //facciones
		{
			new gformat3[320], dialog[2048] = "#ID\tNombre\n", count = 0;
			Loop(id, totalfacciones, 1) if (InfoFaccion[id][fTipo] != 0)
			{
				format(gformat3, sizeof gformat3, "{EBEBEB}%d\t%s\n", id, InfoFaccion[id][fNombre]);
