{
	if (NPCrepara[playerid] != 0) return SendClientMessage(playerid, COLOR_GREY, "* Ya tienes una operación de mecánico activa.");
	if (GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_GREY, "* Debes ser el conductor de un vehículo.");
	if (VehiculoGasolina[playerid] == 1) return SendClientMessage(playerid, COLOR_GREY, "* No puedes llamar un mecánico en este momento.");
	new vehicleid = GetPlayerVehicleID(playerid), costo = 450;
	new Float: cvida;
	GetVehicleHealth(vehicleid, cvida);
	new Percent_damage = floatround(cvida);
	if (Percent_damage >= 520) return SendClientMessage(playerid, COLOR_GREY, "* El health (/dl) del vehículo pasa los 520, lleva el vehículo a un taller.");
	if (GetPlayerCash(playerid) < costo) { SendClientMessageEx(playerid, COLOR_GREY, "* No tienes suficiente dinero ($%s).", diner_coma(costo)); return 1; }
	SetTimerEx("CheckMecanicos", 60000, false, "ddd", playerid, vehicleid, costo);
	SendClientMessage(playerid, COLOR_YELLOW, "Llamaste un mecánico (npc), el mismo reparará un poco la vida del motor de tu vehículo. (60 segundos)");
    NPCrepara[playerid] = 1;
	return 1;
}

forward CheckMecanicos(playerid, vehicleid, costo);
public CheckMecanicos(playerid, vehicleid, costo)
{
	if (GetPlayerCash(playerid) < costo) { SendClientMessageEx(playerid, COLOR_GREY, "* No tienes suficiente dinero ($%s).", diner_coma(costo)); return 1; }
	SetVehicleHealth(vehicleid, 550.0);
	if (veh_gasolina[vehicleid] >= 199) veh_gasolina[vehicleid] = 200;
	else veh_gasolina[vehicleid] += 25;

	GivePlayerCash(playerid, -costo, "cobro mecanico npc");
	new string[190];
	format(string, sizeof string, "** Un mecánico llegó al lugar y revisó las fallas del vehículo de %s.", NombreJugador(playerid));
	ProxDetector(30.0, playerid, string, COLOR_LIGHTGREEN, COLOR_LIGHTGREEN, COLOR_LIGHTGREEN, COLOR_LIGHTGREEN, COLOR_LIGHTGREEN);
    NPCrepara[playerid] = 0;
    SendClientMessage(playerid, COLOR_YELLOW, "Revisaron las fallas del motor (550 hp) y echaron algo de gasolina en el tanque de tu vehículo.");
	return 1;
}


