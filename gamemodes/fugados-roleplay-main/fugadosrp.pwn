cmd:oral(playerid, params[])
{
	if (IsPlayerContrato(playerid, 4) == 0) return SendClientMessage(playerid, COLOR_GREY, "* No tienes el trabajo de Prostituta.");
	if (PutaOral[playerid] != -1) return SendClientMessage(playerid, COLOR_GREY, "* Ya ofreciste el servicio, espera que te respondan.");
	if (!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "* Debes estar dentro de un vehículo para hacer esto.");
	new gdsdu[512];
	if (!CheckTimer(120, TimerProstituta[playerid]))
	{
		new playerid2, precio, vehicleid = GetPlayerVehicleID(playerid);
		if (sscanf(params, "dd", playerid2, precio)) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /oral (JugadorID) (Precio$)");
		if (playerid2 == playerid) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /oral (JugadorID) (Precio$)");
		if (!IsPlayerConnectedEx(playerid2)) return SendClientMessage(playerid, COLOR_GREY, "* El jugador seleccionado no está conectado.");
		if (InfoPersonaje[playerid2][pHorasOn] < 5) return SendClientMessage(playerid, COLOR_GREY, "* Ese jugador debe tener al menos 5 horas de juego.");
		if (ClienteOral[playerid2] != -1) return SendClientMessage(playerid, COLOR_GREY, "* El jugador seleccionado ya le han ofrecido este servicio.");
		if (precio < 50 || precio > 3000) return SendClientMessage(playerid, COLOR_GREY, "* El cobro mínimo por sexo oral es de $50 y el máximo de $3,000.");
		if (!IsPlayerInVehicle(playerid2, vehicleid)) return SendClientMessage(playerid, COLOR_GREY, "* El jugador seleccionado no está dentro del mismo vehículo que tu.");
		if (GetPlayerVehicleSeat(playerid) != 1) return SendClientMessage(playerid, COLOR_GREY, "* Debes estar en el asiento que está al lado del conductor.");
		if (GetPlayerVehicleSeat(playerid2) != 0) return SendClientMessage(playerid, COLOR_GREY, "* El jugador seleccionado debe estar en el asiento del conductor.");
		if (EstaEnBicis(vehicleid) || IsAPlane(vehicleid) || IsABike(vehicleid)) return SendClientMessage(playerid, COLOR_GREY, "* Este vehículo no es el adecuado para este servicio.");
		PrecioOral[playerid2] = precio;
		ClienteOral[playerid2] = playerid;
		PutaOral[playerid] = playerid2;
		format(gdsdu, sizeof gdsdu, "{00AACC}%s{EBEBEB} te ofrece {FFBF00}sexo oral{EBEBEB} por {00CC00}$%s{EBEBEB}\n\
		Si quieres proceder con el servicio presiona '{FFFF00}continuar{EBEBEB}'\n", NombreJugador(playerid), diner_coma(precio));
		Dialog_Show(playerid2, ProstitutaShow, DIALOG_STYLE_MSGBOX, "Prostituta", gdsdu, "Continuar", "Salir");
		SendClientMessage(playerid, COLOR_YELLOW, "Has ofrecido sexo oral, espera la respuesta.");
	}
	else
	{
		SendClientMessageEx(playerid, COLOR_GREY, "Puedes volver a usar este comando en %s.", Segundos_tiempo( CheckTimer(120, TimerProstituta[playerid]) ));
	}
 	return 1;
}

