			SendClientMessage(playerid, colortexture, query);
		}
		case 1:
		{
			if (InfoMueble[id][mTxtModelo2] == 0) return SendClientMessage(playerid, COLOR_GREY, "* Debes agregar una textura antes de elegir un color, slot 2.");
