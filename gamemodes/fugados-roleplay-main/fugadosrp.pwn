	}
	format(string, sizeof string, "{FF2F2F}[AdmCmd]{FFFFFF} %d vehículos fueron reparados por %s(%d).", count, InfoCuenta[playerid][jNombre], playerid);
	SendAdminMessage(0xFFFFFFFF, string);
    return 1;
}

