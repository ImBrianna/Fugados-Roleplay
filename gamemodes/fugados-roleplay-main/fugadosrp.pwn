forward fadeLab(playerid, fase, drug, mano);
public fadeLab(playerid, fase, drug, mano){
	new id = ObtenerLaboratorio(playerid);
	if(id == -1) return SendClientMessage(playerid, COLOR_GREY, "Ya no estás en un laboratorio");

	if(fase == 0){

		if(drug == 1 || drug == 3){
			if(Lab[id][TiempoCreacion2] > 2160){
				SendClientMessageEx(playerid, COLOR_GREY, "Tienes que esperar %0.2f horas para volver a combinar.", floatabs(36.0-float(Lab[id][TiempoCreacion2])/60.0));
				return 1;
			}
		}
		else if(Lab[id][TiempoCreacion1] > 2160){
			SendClientMessageEx(playerid, COLOR_GREY, "Tienes que esperar %0.2f horas para volver a crear droga.", floatabs(36.0-float(Lab[id][TiempoCreacion1])/60.0));
			return 1;
		}
		FadePlayerScreenColour(playerid, 0x000000FF, 2200, 25);
		SetTimerEx("fadeLab", 2500, false, "dddd", playerid, 1, drug, mano);
		TogglePlayerControllable(playerid, 0);
	}else{
		if(drug == 1 || drug == 3) Lab[id][TiempoCreacion2] += 2160;
		else Lab[id][TiempoCreacion1] += 2160;
		FadePlayerScreenColour(playerid, 0x00000000, 2200, 25);
		TogglePlayerControllable(playerid, 1);
		new obj, cant;
		if(drug == 0){ // coca
			SendClientMessage(playerid, COLOR_GREY, "Creaste 60 gramos de cocaína");
			obj = 46;
			cant = 60;
			Lab[id][Tropano] -= 35;
		} else if(drug == 1){ //cracl
			SendClientMessage(playerid, COLOR_GREY, "Creaste 40 gramos de crack");
			obj = 44;
			cant = 40;
		} else if(drug == 2){
			SendClientMessage(playerid, COLOR_GREY, "Creaste 70 gramos de LSD");
			obj = 74;
			cant = 70;
			Lab[id][Ergolina] -= 26;
			Lab[id][Triptina] -= 35;
		} //lsd
		else if (drug == 3){
			//mdma
			SendClientMessage(playerid, COLOR_GREY, "Creaste 70 gramos de MDMA (Revisa el pasillo)");
			tirar_objeto(73, 70, 2537.8347,-1275.9371,23.1925, 277+Lab[id][sID], 22, 0);
			return 1;
		}
		else if(drug == 4){
			//meta
			SendClientMessage(playerid, COLOR_GREY, "Creaste 60 gramos de metanfetamina");
			obj = 45;
			cant = 50;
			Lab[id][Butano] -= 35;
			Lab[id][Efedrina] -= 20;
		}
		else if(drug == 6){
			//  anfeta
			SendClientMessage(playerid, COLOR_GREY, "Creaste 70 gramos de anfetamina");
			obj = 68;
			cant = 70;
			Lab[id][Tropano] -= 35;
			Lab[id][Efedrina] -= 25;
		}
		if(mano == 0){
			InfoPersonaje[playerid][pManoDerecha] = obj;
			InfoPersonaje[playerid][pManoDerechaCant] = cant;
		}else{
			InfoPersonaje[playerid][pManoIzquierda] = obj;
			InfoPersonaje[playerid][pManoIzquierdaCant] = cant;
		}
		ActualizarManos(playerid, 1);
		ActualizarLaboratorio(id);
	}
	return 1;
}
