					{
						DestroyFire(FireID);
						GivePlayerCash(playerid, 550, "apagar incendio");
						SendClientMessage(playerid, COLOR_LIGHTBLUE, "Recibiste $550 por apagar el incendio.");
					}
				}
			}
		}
	}
	//basurero
	if (con_basura_mano[playerid] != 0)
	{
		if (CercaDelVehiculo(playerid, tu_veh_basura[playerid], 8.0))
		{
			if (newkeys == KEY_FIRE)
