cmd:crearlaboratorio(playerid, params[]){
	if (CheckAdminCMD(playerid, 4)) return 1;

	new precio;
	if (sscanf(params, "i", precio)) return SendClientMessage(playerid, COLOR_GREY, "/crearlaboratorio precio");

	new libre = -1;
	for(new i = 1; i < MAX_LABORATORIOS; i++){
		if(Lab[i][Propietario] == -1 && !Lab[i][EnUso]){
			libre = i;
			break;
		}
	}
	if(libre == -1) return SendClientMessage(playerid, COLOR_GREY, "No hay laboratorios disponibles");

	Lab[libre][Propietario] = -1;
	Lab[libre][Llave1l] = -1;
	Lab[libre][Llave2l] = -1;
	Lab[libre][Llave3l] = -1;
	Lab[libre][Venta] = precio;
	Lab[libre][Activo] = false;
	Lab[libre][EnUso] = true;

	GetPlayerPos(playerid, Lab[libre][Entrada][0], Lab[libre][Entrada][1], Lab[libre][Entrada][2]);
	GetPlayerFacingAngle(playerid, Lab[libre][EAngulo]);
	Lab[libre][Salida][0] = 2546.5469;
	Lab[libre][Salida][1] = -1266.3907;
	Lab[libre][Salida][2] = 23.1925;
	Lab[libre][SAngulo] = 177.2992;

	Lab[libre][Ventilacion] = false;
	Lab[libre][Extractor] = false;
	Lab[libre][Proveedor] = false;
	Lab[libre][Equipo] = false;
	Lab[libre][Quimico] = false;

	Lab[libre][Triptina] = 0;
	Lab[libre][Ergolina] = 0;
	Lab[libre][Tropano] = 0;
	Lab[libre][Efedrina] = 0;
	Lab[libre][Butano] = 30;
	Lab[libre][Seguro] = true;

	new que2ry[2000], query[3500];
	if(Lab[libre][EnUso] == 0 && Lab[libre][sID] >= 0){ // Existente
		ActualizarLaboratorio(libre);
	}
	else{ // No existente
		format(que2ry, sizeof que2ry, "INSERT INTO laboratorios (Activo)");
		mysql_format(db_samp, query, sizeof query, "%s VALUES (0)",
		que2ry);
		mysql_tquery(db_samp, query, "OnLabCreate", "i", libre);

		SetTimerEx("updatelab", 300, false, "i", libre);
	}
	SendClientMessageEx(playerid, COLOR_GREY, "Se creó el laboratorio correctamente (%d)(%d$)", libre, Lab[libre][Venta]);
	SendClientMessage(playerid, COLOR_GREY, "Recuerda utilizar /puntolaboratorio para entrega de trailer");
	return 1;
}

SQLLabID(ID){
	for(new i; i < MAX_LABORATORIOS; i++){
		if(Lab[i][sID] == ID) return i;
	}
	return -1;
}

forward updatelab(id);
public updatelab(id){
	ActualizarLaboratorio(id);
}

ActualizarLaboratorio(id){
