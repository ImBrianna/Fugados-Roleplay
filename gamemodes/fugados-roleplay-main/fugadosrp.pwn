					is_mueble_actual[playerid] = 0;
				}
			}
	}
	if (InfoPersonaje[playerid][pEditingMode] == 13) // comando creargraffiti
	{
		new id = edicion_sql_actual[playerid];
		if (id == -1) return SendClientMessage(playerid, -1, "Error edición.");
