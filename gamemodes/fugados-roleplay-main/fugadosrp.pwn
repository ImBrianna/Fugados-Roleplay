cmd:combustible(playerid)
{
	if (GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_GREY, "* Debe ser el conductor de un vehículo.");
	if (VehiculoGasolina[playerid] == 1) return SendClientMessage(playerid, COLOR_GREY, "* Ya estás llenando el tanque de gasolina.");
	new gas = -1;
	Loop(id, totalgasolinas, 1) if (IsPlayerInRangeOfPoint(playerid, 8.0, InfoBidon[id][gsPotX], InfoBidon[id][gsPotY], InfoBidon[id][gsPotZ]) && GetPlayerVirtualWorld(playerid) == InfoBidon[id][gsVirtualWorld] && GetPlayerInterior(playerid) == InfoBidon[id][gsInterior])
	{
		gas = id;
	}
	if (gas == -1) return SendClientMessage(playerid, COLOR_GREY, "* No estás en ningún punto de bidón de gasolina.");
	new vehicleid = GetPlayerVehicleID(playerid), engine, lights, alarm, doors, bonnet, boot, objective;
	if (EstaEnBicis(vehicleid)) return SendClientMessage(playerid, COLOR_WHITE, "AAaaaaa no, tu tienes serios problemaxD¿");
	GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);
	if (engine == VEHICLE_PARAMS_ON) return SendClientMessage(playerid, COLOR_GREY, "* Debes tener el motor apagado para tomar gasolina.");
	if (InfoCuenta[playerid][jVIP] == 0)
	{
		if (GetPlayerCash(playerid) < 115) {	SendClientMessage(playerid, COLOR_GREY, "* No tienes suficiente dinero ($115)."); return 1; }
	}
	if (veh_gasolina[vehicleid] >= 199) return SendClientMessage(playerid, COLOR_GREY, "* El tanque de gasolina del vehículo no aguanta más de 200 litros.");
	GameTextForPlayer(playerid, "~w~Echando ~p~gasolina...", 10000, 3);
	VehiculoGasolina[playerid] = 1;
	SetTimerEx("llenando_gasolina", 6000, false, "dd", playerid, vehicleid);
	return 1;
}
