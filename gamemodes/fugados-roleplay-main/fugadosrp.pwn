    return 0;
}


forward ResetearTrailero(trailerId);
public ResetearTrailero(trailerId){
    if(traInfo[trailerId][Activo]){
        if(TraileroEnTrailer(trailerId) == -1){
            resetTrailer(trailerId);
        }
		else {
			if (traInfo[trailerId][TimerActivo]){
				KillTimer(traInfo[trailerId][TimerReset]);
				if(traInfo[trailerId][cTipo] == 3) traInfo[trailerId][TimerReset] =  SetTimerEx("ResetearTrailero", 600000, false, "d", trailerId); // 10mins
				else traInfo[trailerId][TimerReset] =  SetTimerEx("ResetearTrailero", 180000, false, "d", trailerId);
			}
		}
    }
    return 1;
}

resetTrailer(trailerId){

    if(traInfo[trailerId][Conductor] != -1) // Alguien estaba en ese vehículo.
    {
		new playerid2 = SQLConectado(traInfo[trailerId][Conductor]);

		if(playerid2 != -1){
			pDVar[playerid2][tAsignado] = -1;
			pDVar[playerid2][tDistancia] = -1;
			pDVar[playerid2][pAsignado] = -1;
			pDVar[playerid2][Activo] = 0;
			DisablePlayerCheckpoint(playerid2);
			DisablePlayerCheckpoint(playerid2);
			SendClientMessage(playerid2, COLOR_GREY, "* Tu carga se ha reseteado por que la has abandonado o no se encontró algún punto de entrega.");
		}

		guardar_int_mysql("personajes", "trailerAsignado", traInfo[trailerId][Conductor], -1);
        guardar_int_mysql("personajes", "PtrailerAsignado", traInfo[trailerId][Conductor], -1);
        guardar_int_mysql("personajes", "trailerDistancia", traInfo[trailerId][Conductor], -1);
        guardar_int_mysql("personajes", "trailerActivo", traInfo[trailerId][Conductor], 0);
    }

	DestroyVehicle(traInfo[trailerId][VehID]);
	traInfo[trailerId][VehID] = INVALID_VEHICLE_ID;
    traInfo[trailerId][Conductor] = -1;
    traInfo[trailerId][Activo] = false;
    if(traInfo[trailerId][TimerActivo]) KillTimer(traInfo[trailerId][TimerReset]);
    traInfo[trailerId][TimerActivo] = false;
    traInfo[trailerId][Modelo] = 0;
}

desbugeartrailero(playerid){
    for(new i; i < MAX_TCARGA; i++){
        if(InfoPersonaje[playerid][pID] == traInfo[i][Conductor]) return;
    }
    pDVar[playerid][tAsignado] = -1;
    pDVar[playerid][tDistancia] = -1;
    pDVar[playerid][pAsignado] = -1;
    pDVar[playerid][Activo] = false;
	return;
}



