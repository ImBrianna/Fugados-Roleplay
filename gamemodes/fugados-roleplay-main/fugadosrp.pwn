#define MAX_INTENTOS_PUNTO 70

AsignarPuntoTrailer(playerid){
	DisablePlayerCheckpoint(playerid);
	// asegurar la distancia correcta
	new intentos = 0, dist;
	if(traInfo[pDVar[playerid][tAsignado]][cTipo] == 3){ // laboratorio
		if(pDroga[playerid][SelEntrega] == 1 ) {
			if(pDroga[playerid][LabID] == -1) return SendClientMessage(playerid, COLOR_GREY, "Ya no tienes este laboratorio.");
			pDVar[playerid][pAsignado] = SQLLabID(pDroga[playerid][LabID]);
		} else{
			if(pDroga[playerid][LlaveLab] == -1) return SendClientMessage(playerid, COLOR_GREY, "Ya no tienes este laboratorio.");
			pDVar[playerid][pAsignado] = SQLLabID(pDroga[playerid][LlaveLab]);
			resetTrailer(pDVar[playerid][tAsignado]);
		}
		
		pDVar[playerid][tDistancia] += dist;

		SendClientMessage(playerid, COLOR_GREY, "Lleva la carga a tu laboratorio.");
		new snitch = random(8);
		if(snitch == 1){
			new msg[256];
			new Float:x, Float:y, Float:z;
			GetPlayerPos(playerid, x,y,z);
			Get2DZone(msg, MAX_ZONE_NAME, x,y,z);
			format(msg, sizeof msg, "[CENTRAL] Se ha reportado un trailer con contenido ilegal saliendo en %s", msg);
			SendRadioMessage(1, 0x9B9BFFFF, msg);
			SendRadioMessage(4, 0x9B9BFFFF, msg);
		}
		SetPVarInt(playerid, "EnCheckPoint", 279);
		SetPlayerCheckpoint(playerid, Lab[pDVar[playerid][pAsignado]][Entrega][0], Lab[pDVar[playerid][pAsignado]][Entrega][1], Lab[pDVar[playerid][pAsignado]][Entrega][2], 5.0);
	}
	else {
		while(intentos < MAX_INTENTOS_PUNTO){
			if(pDVar[playerid][tCarga] == 2){
				pDVar[playerid][pAsignado] = random(TOTAL_TENT);
				dist = floatround(GetVehicleDistanceFromPoint(GetPlayerVehicleID(playerid), traiEntrega[pDVar[playerid][pAsignado]][0], traiEntrega[pDVar[playerid][pAsignado]][1], traiEntrega[pDVar[playerid][pAsignado]][2]));
				if(pDVar[playerid][tDistancia]+dist < 6000){
					pDVar[playerid][tDistancia] += dist;
					intentos = 99;
				}
			}else{ // +6km
				pDVar[playerid][pAsignado] = random(TOTAL_TENT);
				dist = floatround(GetVehicleDistanceFromPoint(GetPlayerVehicleID(playerid), traiEntrega[pDVar[playerid][pAsignado]][0], traiEntrega[pDVar[playerid][pAsignado]][1], traiEntrega[pDVar[playerid][pAsignado]][2]));
				if(pDVar[playerid][tDistancia]+dist > 6000){
					pDVar[playerid][tDistancia] += dist;
					intentos = 99;
				}
			}
			intentos++;
		}
		if(intentos != 100){
			SendClientMessage(playerid, COLOR_GREY, "* No hay ningún punto disponible, se ha cancelado el viaje");
			resetTrailer(pDVar[playerid][tAsignado]);
			return 0;
		}

		if(pDVar[playerid][tCarga] == 3){
			new snitch = random(3);
			if(snitch == 1 && pDroga[playerid][TiempoMetanfetamina] == 0){
				new msg[256];
				Get2DZone(msg, MAX_ZONE_NAME, traiEntrega[pDVar[playerid][pAsignado]][0], traiEntrega[pDVar[playerid][pAsignado]][1], traiEntrega[pDVar[playerid][pAsignado]][2]);
				format(msg, sizeof msg, "[CENTRAL] Se ha reportado un trailer transportando contenido ilegal con destino %s", msg);
				SendRadioMessage(1, 0x9B9BFFFF, msg);
				SendRadioMessage(4, 0x9B9BFFFF, msg);
			}
		}
		SetPlayerCheckpoint(playerid, traiEntrega[pDVar[playerid][pAsignado]][0], traiEntrega[pDVar[playerid][pAsignado]][1], traiEntrega[pDVar[playerid][pAsignado]][2], 5.0);
		SetPVarInt(playerid, "EnCheckPoint", 277);
	}

	guardar_int_mysql("personajes", "trailerDistancia", InfoPersonaje[playerid][pID], pDVar[playerid][tDistancia]);
	guardar_int_mysql("personajes", "PtrailerAsignado", InfoPersonaje[playerid][pID], pDVar[playerid][pAsignado]);

	MostrarViajeTrailero(playerid, playerid);
	return 1;
}

public OnTrailerUpdate(playerid, vehicleid)
{
    for(new i; i < MAX_TCARGA; i++){
        if(IsValidVehicle(traInfo[i][VehID])){
            if(vehicleid == traInfo[i][VehID]){
                if(traInfo[i][Conductor] == InfoPersonaje[playerid][pID]){
                    if(pDVar[playerid][pAsignado] == -1){
						if(!AsignarPuntoTrailer(playerid)) return 1;
						new lugar[256];
						if(traInfo[pDVar[playerid][tAsignado]][cTipo] == 3){
							Get2DZone(lugar, MAX_ZONE_NAME, Lab[pDVar[playerid][pAsignado]][Entrega][0], Lab[pDVar[playerid][pAsignado]][Entrega][1], Lab[pDVar[playerid][pAsignado]][Entrega][2]);
							format(lugar, sizeof lugar, "El destino de la carga está ubicado en %s, ve a su ubicación.", lugar);
						}
						else {
							Get2DZone(lugar, MAX_ZONE_NAME, traiEntrega[pDVar[playerid][pAsignado]][0], traiEntrega[pDVar[playerid][pAsignado]][1], traiEntrega[pDVar[playerid][pAsignado]][2]);
							format(lugar, sizeof lugar, "El destino de la carga está ubicado en %s, ve a su ubicación.", lugar);
						}
						SendClientMessage(playerid, COLOR_GREY, lugar);
                    }
                }
                else {
                    if(IsPlayerInAnyVehicle(playerid)){
                        new vId = GetPlayerVehicleID(playerid);
                        if(GetVehicleTrailer(vId) == traInfo[i][VehID])
                        {
                            DetachTrailerFromVehicle(vId);
                            SendClientMessage(playerid, COLOR_GREY, "* No puedes tomar la carga de otro jugador!");
                            return 0;
                        }
                    }
                }
            }
        }
    }
    return 1;
}

forward QuitarTrailer(playerid, fase);
public QuitarTrailer(playerid, fase){
	if(fase == 0){
		new trailerId = pDVar[playerid][tAsignado];
		new pVeh = GetPlayerVehicleID(playerid);
		if(pVeh > 0 ) DetachTrailerFromVehicle(pVeh);
		DestroyVehicle(traInfo[trailerId][VehID]);
		traInfo[trailerId][VehID] = INVALID_VEHICLE_ID;
		if(traInfo[trailerId][cTipo] == 3){
			if(!IsPlayerConnectedEx(playerid)) return 1;
			SetTimerEx("QuitarTrailer", 1500, false, "dd", playerid, 2);
		}else{
			traInfo[trailerId][Conductor] = -1;
			traInfo[trailerId][Activo] = false;
			if(traInfo[trailerId][TimerActivo]) KillTimer(traInfo[trailerId][TimerReset]);
			traInfo[trailerId][TimerActivo] = false;
			traInfo[trailerId][Modelo] = 0;
			pDVar[playerid][tAsignado] = -1;
			if(!IsPlayerConnectedEx(playerid)) return 1;
			SetTimerEx("QuitarTrailer", 1500, false, "dd", playerid, 1);
		}

	}
	else if(fase == 1){
		if(!IsPlayerConnectedEx(playerid)) return 1;
		TogglePlayerControllable(playerid, 1);
		FadePlayerScreenColour(playerid, 0x00000000, 2500, 30);
		SendClientMessage(playerid, COLOR_GREY, "Regresa el trailer a la central.");
        SetPlayerCheckpoint(playerid, puntoEntrega[0], puntoEntrega[1], puntoEntrega[2], 5.0);
		SetPVarInt(playerid, "EnCheckPoint", 278);
	}
	else if(fase == 2){
		new trailerId = pDVar[playerid][tAsignado];
		if(!IsPlayerConnectedEx(playerid)) return 1;
		TogglePlayerControllable(playerid, 1);
		FadePlayerScreenColour(playerid, 0x00000000, 2500, 30);
		SendClientMessage(playerid, COLOR_GREY, "Entregaste los materiales al laboratorio.");
		SendClientMessage(playerid, COLOR_GREY, "Deberías de alejar el camión del lugar.");
		pDVar[playerid][tAsignado] = -1;
		pDVar[playerid][tDistancia] = -1;
		pDVar[playerid][pAsignado] = -1;
		pDVar[playerid][Activo] = false;

		traInfo[trailerId][Conductor] = -1;
		traInfo[trailerId][Activo] = false;
		traInfo[trailerId][TimerActivo] = false;
		traInfo[trailerId][Modelo] = 0;
		if(traInfo[trailerId][TimerActivo]) KillTimer(traInfo[trailerId][TimerReset]);
	}
	return 1;
}

Dialog:ShowTrailero(playerid, response, listitem, inputtext[])
{
	if (!response) return 1;
	pDVar[playerid][tCarga] = listitem+1;
	if(pDVar[playerid][tCarga] == 3 && pDVar[playerid][timerIlegal] != 0 ){
		return SendClientMessageEx(playerid, COLOR_GREY, "* Aún no puedes hacer otro viaje ilegal, espera %d minutos.", pDVar[playerid][timerIlegal]);
	}
	new tId = Trailer_Libre(), randPos;
	if(tId == -1) return SendClientMessage(playerid, COLOR_GREY, "* Todos los trailers están ocupados!");
    new modelId, intentos;
    modelId = TraModels[random(8)];
    randPos = random(TOTAL_TPICKS);
    while(intentos < MAX_TRY){
        randPos = random(TOTAL_TPICKS);
        intentos++;
        if(!isAnyVehicleInPos(traiPick[randPos][0], traiPick[randPos][1], traiPick[randPos][2], 5.0)) intentos = 50;
    }
    if(intentos == 30) return SendClientMessage(playerid, COLOR_GREY, "* Todos los puntos de carga están ocupados!");

    new dist = floatround(GetVehicleDistanceFromPoint(GetPlayerVehicleID(playerid), traiPick[randPos][0], traiPick[randPos][1], traiPick[randPos][2]));
    pDVar[playerid][tAsignado] = tId;
    pDVar[playerid][tDistancia] = dist;
    pDVar[playerid][pAsignado] = -1;
    pDVar[playerid][Activo] = true;

    guardar_int_mysql("personajes", "trailerAsignado", InfoPersonaje[playerid][pID], tId);
    guardar_int_mysql("personajes", "trailerDistancia", InfoPersonaje[playerid][pID], dist);
    guardar_int_mysql("personajes", "trailerActivo", InfoPersonaje[playerid][pID], 1);
    guardar_int_mysql("personajes", "trailerTipo", InfoPersonaje[playerid][pID], pDVar[playerid][tCarga]);

    traInfo[tId][VehID] = CreateVehicle(modelId, traiPick[randPos][0], traiPick[randPos][1], traiPick[randPos][2], traiPick[randPos][3]+2.5, random(10)+1, random(10)+1, 60, 0);
    traInfo[tId][Modelo] = modelId;
    traInfo[tId][Activo] = true;
    traInfo[tId][Conductor] = InfoPersonaje[playerid][pID];

    new lugar[256];
    Get2DZone(lugar, MAX_ZONE_NAME, traiPick[randPos][0], traiPick[randPos][1], traiPick[randPos][2]);

	traInfo[tId][Tipo] = random(MAX_CARGAS_SIZE);
	if(listitem == 2) traInfo[tId][cTipo] = 2;
	else traInfo[tId][cTipo] = 1;

	MostrarViajeTrailero(playerid, playerid);
	if(pDVar[playerid][tCarga] == 3) SendClientMessageEx(playerid, COLOR_GREY, " ** Estás llevando una carga ilegal, en realidad transportarás {4ab852}%s.", (traInfo[tId][Modelo] == 584) ? cTrailer[2][traInfo[tId][Tipo]] : cTrailer[3][traInfo[tId][Tipo]]);

    Get2DZone(lugar, MAX_ZONE_NAME, traiPick[randPos][0], traiPick[randPos][1], traiPick[randPos][2]);
    format(lugar, sizeof lugar, "Se te ha asignado un contenedor en %s, ve a su ubicación y remolca con tu vehículo.", lugar);
    SendClientMessage(playerid, COLOR_GREY, lugar);

    SetPlayerCheckpoint(playerid, traiPick[randPos][0], traiPick[randPos][1], traiPick[randPos][2], 5.0);
    traInfo[tId][TimerReset] =  SetTimerEx("ResetearTrailero", 240000, false, "d", tId); // 4 minutos para llegar a la carga y tomarla
	traInfo[tId][TimerActivo] = true;

	return 1;
}

