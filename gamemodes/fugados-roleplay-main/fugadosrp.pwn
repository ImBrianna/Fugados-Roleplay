		case 47,48,49: format(mensaje, sizeof mensaje, "%s ({00AACC}%d usos{FFFFFF})", InfoInventario[id_objeto][NombreObjeto], cantidad);
		case 55: format(mensaje, sizeof mensaje, "%s ({00AACC}ID %d{FFFFFF})", InfoInventario[id_objeto][NombreObjeto], cantidad);
