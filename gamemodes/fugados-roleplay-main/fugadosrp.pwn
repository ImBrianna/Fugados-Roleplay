mostrar_velocimetro(playerid, vehicleid)
{
	if (InfoPersonaje[playerid][pTogVelocimetro] == 0) return 1;
	if (vehicleid == 0) return 1;
	//actived
	show_velocimetro[playerid] = 1;
	//new's
	new colorsito = 1, id = verificar_id_vehiculo(vehicleid);
	if (id != -1) colorsito = InfoVehiculo[id][vColor_1];
	new vehcolor = VehicleColoursTableRGBA[colorsito];
	uwu_color_veh[vehicleid] = colorsito;
	new gvel[256];
	if (EstaEnBicis(vehicleid))
	{
		//velocimetro
		PlayerTextDrawColor(playerid, TD_velocimetro[playerid][0], vehcolor);
		format(gvel, sizeof gvel, "%03d", floatround(GetVehicleSpeed(vehicleid)));
		PlayerTextDrawSetStringEx(playerid, TD_velocimetro[playerid][0], gvel);
		PlayerTextDrawShow(playerid, TD_velocimetro[playerid][0]);
		//modelo
		PlayerTextDrawColor(playerid, TD_velocimetro[playerid][1], vehcolor);
		format(gvel, sizeof gvel, "%s", Veh_Textdraw(GetVehicleName(vehicleid)));
		PlayerTextDrawSetStringEx(playerid, TD_velocimetro[playerid][1], gvel);
		PlayerTextDrawShow(playerid, TD_velocimetro[playerid][1]);
		//seguro
		if (InfoVehiculo[id][vSeguro] == 0) format(gvel, sizeof gvel, "Seguro: ~g~~h~Abierto");
		else format(gvel, sizeof gvel, "Seguro: ~r~~h~Cerrado");
		PlayerTextDrawSetStringEx(playerid, TD_velocimetro[playerid][2], gvel);
		PlayerTextDrawShow(playerid, TD_velocimetro[playerid][2]);
	}
	else
	{
		//km/h
		PlayerTextDrawColor(playerid, TD_velocimetro[playerid][0], vehcolor);
		format(gvel, sizeof gvel, "%03d", floatround(GetVehicleSpeed(vehicleid)));
		PlayerTextDrawSetStringEx(playerid, TD_velocimetro[playerid][0], gvel);
		PlayerTextDrawShow(playerid, TD_velocimetro[playerid][0]);
		//gasolina
		format(gvel, sizeof gvel, "Gasolina:_~g~%03d", veh_gasolina[vehicleid]);
		PlayerTextDrawSetStringEx(playerid, TD_velocimetro[playerid][1], gvel);
		PlayerTextDrawShow(playerid, TD_velocimetro[playerid][1]);
		//seguro
		if (InfoVehiculo[id][vSeguro] == 0) 	format(gvel, sizeof gvel, "Seguro: ~g~~h~Abierto");
		else format(gvel, sizeof gvel, "Seguro: ~r~~h~Cerrado");
		PlayerTextDrawSetStringEx(playerid, TD_velocimetro[playerid][2], gvel);
		PlayerTextDrawShow(playerid, TD_velocimetro[playerid][2]);
		//modelo
		PlayerTextDrawColor(playerid, TD_velocimetro[playerid][3], vehcolor);
		format(gvel, sizeof gvel, "%s", Veh_Textdraw(GetVehicleName(vehicleid)));
		PlayerTextDrawSetStringEx(playerid, TD_velocimetro[playerid][3], gvel);
		PlayerTextDrawShow(playerid, TD_velocimetro[playerid][3]);
	}
	//km/h
	PlayerTextDrawShow(playerid, TD_velocimetro[playerid][4]);
	return 1;
}

