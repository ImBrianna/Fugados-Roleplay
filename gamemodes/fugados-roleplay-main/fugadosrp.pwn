stock IsValidCaracterAnunce(const password[])
{
    for(new i = 0; password[i] != EOS; ++i)
    {
        switch(password[i])
        {
            case '0'..'9', 'A'..'Z', 'a'..'z', '.', ',', '(', ')', '[', ']', ' ', '_', '$', 'ñ': continue;
            default: return 0;
        }
    }
    return 1;
}

