funcion tiene_carga(playerid, idobj)
{
	new izq = InfoPersonaje[playerid][pManoIzquierda];
	if (izq == idobj)
	{
		return idobj;
	}
	Loop(e, 9, 0) if (InfoPersonaje[playerid][pBolsillo][e] == idobj)
	{
		return idobj;
	}
	return 0;
}

cmd:cargar(playerid) //poner cargador
{
	if (!CheckTimer(3, RecargarTiempo[playerid]))
	{
		if (InfoPersonaje[playerid][pManoDerechaCant] == InfoInventario[InfoPersonaje[playerid][pManoDerecha]][MaxCantidad]) return SendClientMessage(playerid, COLOR_GREY, "* El arma que tienes en mano tiene el máximo de balas.");
		new derecha = InfoPersonaje[playerid][pManoDerecha], derechaCant = InfoPersonaje[playerid][pManoDerechaCant], izquierda = InfoPersonaje[playerid][pManoIzquierda], izquierdaCant = InfoPersonaje[playerid][pManoIzquierdaCant];
		if (InfoInventario[derecha][IDArma] > 0 && InfoInventario[derecha][MaxCantidad] > 1) //tiene un arma en mano derecha
		{
			ActualizarManos(playerid);
			new cargador, ArmaID = InfoInventario[derecha][IDArma];
			switch (derecha)
			{
				case 19: cargador = 56;
				case 20: cargador = 56;
				case 21: cargador = 57;
				case 22: cargador = 64;
				case 23: cargador = 64;
				case 24: cargador = 64;
				case 25: cargador = 58;
				case 26: cargador = 59;
				case 27: cargador = 61;
				case 28: cargador = 62;
				case 29: cargador = 60;
				case 30: cargador = 63;
				case 31: cargador = 63;
			}
			new objeto = tiene_carga(playerid, cargador);
			if (objeto != cargador) { SendClientMessage(playerid, COLOR_GREY, "* No tienes ningún cargador para esta arma."); return 1; }

			if (izquierda != cargador) //función bolsillos
			{
				new ranura = tiene_objeto(playerid, cargador);
				if (InfoPersonaje[playerid][pBolsilloCant][ranura] == 0) return SendClientMessage(playerid, COLOR_GREY, "* No puedes usar un cargador vacío.");
				if (InfoInventario[derecha][MaxCantidad] >= derechaCant+InfoPersonaje[playerid][pBolsilloCant][ranura]) SendClientMessageEx(playerid, COLOR_YELLOW, "Cargas tu %s con %d balas.", InfoInventario[derecha][NombreObjeto], InfoPersonaje[playerid][pBolsilloCant][ranura]);
				else SendClientMessageEx(playerid, COLOR_YELLOW, "Cargas tu %s con %d balas.", InfoInventario[derecha][NombreObjeto], InfoInventario[derecha][MaxCantidad]-InfoPersonaje[playerid][pManoDerechaCant]);

				ResetPlayerWeapons(playerid);
				GivePlayerWeapon(playerid, ArmaID, InfoPersonaje[playerid][pBolsilloCant][ranura]+derechaCant);
				InfoPersonaje[playerid][pManoDerechaCant] = InfoPersonaje[playerid][pBolsilloCant][ranura]+derechaCant;
				InfoPersonaje[playerid][pBolsillo][ranura] = 0;
				InfoPersonaje[playerid][pBolsilloCant][ranura] = 0;
				RecargarTiempo[playerid] = gettime();
				Anti[playerid][BalasW] = 0;
				ActualizarManos(playerid, 1);
				return 1;
			}
			else //función mano izquierda
			{
				if (izquierdaCant == 0) return SendClientMessage(playerid, COLOR_GREY, "* No puedes usar un cargador vacío.");
				if (InfoInventario[derecha][MaxCantidad] >= derechaCant+izquierdaCant) SendClientMessageEx(playerid, COLOR_YELLOW, "Cargas tu %s con %d balas.", InfoInventario[derecha][NombreObjeto], izquierdaCant);
				else SendClientMessageEx(playerid, COLOR_YELLOW, "Cargas tu %s con %d balas.", InfoInventario[derecha][NombreObjeto], InfoInventario[derecha][MaxCantidad]-InfoPersonaje[playerid][pManoDerechaCant]);
				ResetPlayerWeapons(playerid);
				GivePlayerWeapon(playerid, ArmaID, izquierdaCant+derechaCant);
				InfoPersonaje[playerid][pManoDerechaCant] = izquierdaCant+derechaCant;
				InfoPersonaje[playerid][pManoIzquierda] = 0;
				InfoPersonaje[playerid][pManoIzquierdaCant] = 0;
				RemovePlayerAttachedObject(playerid, 8);
				RecargarTiempo[playerid] = gettime();
				Anti[playerid][BalasW] = 0;
				ActualizarManos(playerid, 1);
				return 1;
			}

		}
	}
	else
	{
		SendClientMessageEx(playerid, COLOR_GREY, "Puedes volver a usar este comando en %s.", Segundos_tiempo( CheckTimer(3, RecargarTiempo[playerid]) ));
	}
	return 1;
}

cmd:caja(playerid, params[])
{
	if (InfoPersonaje[playerid][pManoDerecha] == 55) // caja normal
	{
		new id = InfoPersonaje[playerid][pManoDerechaCant];
		if (sscanf(params, "s[32]", params))
		{
			SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /caja (Opción)");
			SendClientMessage(playerid, COLOR_GREY, "Opciones: ver, sacar, guardar");
			return 1;
		}
		if (!strcmp(params, "ver", true, 3))
		{
			VerCaja(playerid, id);
		}
		else if (!strcmp(params, "sacar", true, 5))
		{
			if (InfoPersonaje[playerid][pManoIzquierda] != 0) return SendClientMessage(playerid, COLOR_GREY, "* Posees la mano izquierda ocupada.");
			new item;
			if (sscanf(params, "s[32]i", params, item))
			{
				new count = 0;
				Loop(x, 10, 0) if (InfoCaja[id][cjObjeto][x] != 0) count++;
				if (count == 0) return SendClientMessage(playerid, COLOR_GREY, "No tienes nada en la caja.");
				SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /caja sacar (slot)");
				Loop(x, 9, 0) if (InfoCaja[id][cjObjeto][x] != 0)
				{
					SendClientMessageEx(playerid, 0xEBEBEBFF, "Tienes en {00AACC}slot %d{EBEBEB}: %s", x+1, slot_objeto(InfoCaja[id][cjObjeto][x], InfoCaja[id][cjObjetoCant][x]));
				}
				return 1;
			}
			if (item < 1 || item > 10) { SendClientMessage(playerid, COLOR_GREY, "El número de slot debe estar entre 1 y 10."); return 1; }
			SacarCaja(playerid, item - 1, id);
		}
		else if (!strcmp(params, "guardar", true, 7))
		{
			if (InfoPersonaje[playerid][pManoIzquierda] == 0) return SendClientMessage(playerid, COLOR_GREY, "* No tienes nada en la mano izquierda.");
			GuardarCaja(playerid, id);
		}
		return 1;
	}
	if (InfoPersonaje[playerid][pManoDerecha] == 67) // sixpack de cerveza
	{
		if (sscanf(params, "s[32]", params))
		{
			SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /caja (Opción)");
			SendClientMessage(playerid, COLOR_GREY, "Opciones: sacar");
			return 1;
		}
		if (!strcmp(params, "sacar", true, 5))
		{
			if (InfoPersonaje[playerid][pManoIzquierda] != 0) return SendClientMessage(playerid, COLOR_GREY, "* Posees la mano izquierda ocupada.");
			new objeto = 65;
			InfoPersonaje[playerid][pManoDerechaCant]--;
			InfoPersonaje[playerid][pManoIzquierda] = objeto;
			PonerObjeto(playerid, 2, objeto);
			InfoPersonaje[playerid][pManoIzquierdaCant] = InfoInventario[objeto][DefaultCantidad];
			ActualizarManos(playerid, 1);
			SendClientMessage(playerid, 0x828BFDFF, "Sacas una botella de cerveza del sixpack. (/beber)");
		}
		return 1;
	}
	SendClientMessage(playerid, COLOR_GREY, "* No tienes ninguna caja en tu mano derecha.");
	return 1;
}

