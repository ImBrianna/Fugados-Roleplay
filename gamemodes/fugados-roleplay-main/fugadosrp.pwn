Dialog:IniciarSesion(playerid, response, listitem, inputtext[])
{
	if (!response) return Dialog_Show(playerid, ShowLogin, DIALOG_STYLE_LIST, "{00AACC}Servercito {FFFFFF}Roleplay - {00AACC}v"#version_actual"", "Iniciar sesión\nRegistrarse\nNotas de versión\nDesconectarse", ">>", "");
	new newid = -1, query[256];
	//verificación
	if (VerificarPersonaje(inputtext))
	{
		rebel_int(0, "CuentaID", newid);
		cache_unset_active();
	}
	//comprobar
	if (newid != -1) mysql_format(db_samp, query, sizeof query, "SELECT * FROM cuentas WHERE Nombre = '%e' OR ID = '%d' LIMIT 1", inputtext, newid);
	else mysql_format(db_samp, query, sizeof query, "SELECT * FROM cuentas WHERE Nombre = '%e' LIMIT 1", inputtext);
	new Cache:result;
	result = mysql_query(db_samp, query);
	//muestra
	if (cache_num_rows() > 0)
	{
		rebel_int(0, "ID", InfoCuenta[playerid][jID]);
		rebel_string(0, "Nombre", InfoCuenta[playerid][jNombre], MAX_PLAYER_NAME);
		foreach(new p: Player) if (conectado[p] == 1 && strcmp(InfoCuenta[playerid][jNombre], InfoCuenta[p][jNombre], true) == 0)
		{
			Kick(playerid);
			SendClientMessage(p, 0xFF6347AA, "Alguien intento entrar hace poco a tu cuenta.");
			return 1;
		}
		rebel_string(0, "Clave", InfoCuenta[playerid][jClave], 65);
		rebel_string(0, "Salt", InfoCuenta[playerid][jSalt], 11);
		cache_unset_active();
		conectado[playerid] = 1;
		new gformat[512];
		format(gformat, sizeof gformat, "{FFFFFF}Se ha identificado la cuenta {00AACC}%s{FFFFFF}\n\
		Escribe la contraseña correcta de la cuenta y presiona {00AACC}Ingresar{FFFFFF}.\n\n\
		Si no es tu cuenta la identificada, puedes dejar el servidor haciendo clic en {00AACC}salir{FFFFFF}\n", InfoCuenta[playerid][jNombre]);
		Dialog_Show(playerid, IniciarSesion2, DIALOG_STYLE_PASSWORD, "{FFFFFF}Contraseña", gformat, "Ingresar", "Salir");
	}
	else
	{
		SendClientMessage(playerid, 0xEBEBEBFF, "El {00AACC}nombre de usuario o personaje{EBEBEB} no existente o inválidos.");
		Dialog_Show(playerid, IniciarSesion, DIALOG_STYLE_INPUT, "{FFFFFF}Identificación", "{FFFFFF}Introduce un {00AACC}nombre de usuario o personaje{FFFFFF} para identificarte:\n", "Continuar", "Volver");
	}
	cache_delete(result);
	return 1;
}

