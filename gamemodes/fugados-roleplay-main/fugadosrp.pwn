cmd:darlicencia(playerid, params[])
{
	if (!leader_faccion(playerid, 1, 4)) return SendClientMessage(playerid, COLOR_GREY, "Sólo líder de LSPD o LSCSD.");
	new playerid2, precio;
	if (sscanf(params, "dd", playerid2, precio)) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /darlicencia (JugadorID) (Precio)");
	if (!IsPlayerConnectedEx(playerid2)) return SendClientMessage(playerid, COLOR_GREY, "Esa ID es inválida, jugador desconectado.");
	if (!CercaDelMarica(8.0, playerid, playerid2)) return SendClientMessage(playerid, COLOR_GREY, "Muy lejos de el jugador.");
	if (InfoPersonaje[playerid2][pLicencia][3] != 0) return SendClientMessage(playerid, COLOR_WHITE, "Este jugador ya tiene una licencia de armas trámitada.");
	if (precio < 0 || precio > 50000) return SendClientMessage(playerid, COLOR_WHITE, "El precio máximo para la licencia de armas es 50 mil dólares.");
	if (Facturing[playerid2] == 1) return SendClientMessage(playerid, COLOR_WHITE, "Este jugador ya tiene una petición pendiente.");
	if (InfoPersonaje[playerid2][pHorasOn] < 5) return SendClientMessage(playerid, COLOR_GRAD2, "[ERROR] Este usuario no tiene más de 5 horas de juego o dispone de restricción de armas.");
	new string[290];
	if (GetPlayerCash(playerid2) < precio)
	{
		format(string, sizeof string, "* Esa persona no dispone de {FFFF00}%s${FFFFFF} para pagar la licencia de armas.", diner_coma(precio));
		SendClientMessage(playerid, COLOR_WHITE, string);
		return 1;
	}
	format(string, sizeof string, "* Oficial {3052DC}%s{EBEBEB} te ofrece una licencia de armas por {00CC00}$%s{EBEBEB}.", NombreJugador(playerid), diner_coma(precio));
	SendClientMessage(playerid2, 0xEBEBEBFF, string);
	SendClientMessage(playerid2, 0xEBEBEBFF, "Utiliza {00AACC}/aceptar licencia{EBEBEB} ó {00AACC}/cancelar licencia{EBEBEB}.");
	SendClientMessage(playerid, COLOR_WHITE, "Espera la respuesta del sujeto, espere un momento.");
	NombreLicencia[playerid2] = playerid;
	PrecioLicencia[playerid2] = precio;
	Facturing[playerid2] = 1; // ID ocupado en la tarea.
	return 1;
}

