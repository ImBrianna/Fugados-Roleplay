cmd:desrentarveh(playerid)
{
	if (GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, COLOR_GREY, "Debes ser el conductor de un vehículo.");
	new vehicleid = GetPlayerVehicleID(playerid);
	if (Vehiculo_Renta(vehicleid) == 0) return SendClientMessage(playerid, COLOR_GREY, "No estás dentro de un vehículo rentable.");
	new id = verificar_id_vehiculo(vehicleid);
	if (InfoVehiculo[id][vRentaTiempo] == 0) return SendClientMessage(playerid, COLOR_GREY, "Este vehículo no está rentado.");
	if (strcmp(NombreDeCuenta(playerid), InfoVehiculo[id][vRentaDueno], true) == 0)
	{
		foreach(new p: Player) if (IsPlayerInVehicle(p, vehicleid))
		{
			if (p != playerid) SendClientMessage(p, COLOR_YELLOW, "El vehículo en el que estabas fue respawneado, el dueño lo desrentó.");
		}
		InfoVehiculo[id][vRentaTiempo] = 0;
		alm(InfoVehiculo[id][vRentaDueno], "none");
		SetVehicleToRespawn(vehicleid);
		GivePlayerCash(playerid, 70, "desrentarveh");
		SendClientMessage(playerid, COLOR_YELLOW, "Has dejado de rentar un vehículo, se te devolvió 70$.");
		return 1;
	}
	SendClientMessage(playerid, COLOR_GREY, "* Este vehículo no está rentado por ti.");
	return 1;
}
