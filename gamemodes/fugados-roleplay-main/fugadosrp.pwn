Dialog:Compartir2Casa(playerid, response, listitem, inputtext[])
{
	if (!response) return CasaShareShow(playerid);
	new n = EnCasa(playerid);
	if (n == -1) return SendClientMessage(playerid, COLOR_GREY, "* No estás en ninguna casa.");
	if (LlavesCasa(playerid, n) == 0) return SendClientMessage(playerid, COLOR_GREY, "* No tienes las llaves de esta casa.");
	if (!isnumeric(inputtext)) return SendClientMessage(playerid, COLOR_GREY, "* Sólo puedes ingresar números, intenta de nuevo.");
	new playerid2 = strval(inputtext);
	if (playerid == playerid2) return SendClientMessage(playerid, COLOR_GREY, "* No puedes compartir permisos de la casa a ti mismo.");
	if (!IsPlayerConnectedEx(playerid2)) return SendClientMessage(playerid, COLOR_GREY, "* El jugador seleccionado no se encuentra conectado.");
	if (strmatch(InfoCasa[n][cCompartida1], InfoPersonaje[playerid2][pNombre_Apellido])) { SendClientMessage(playerid, COLOR_GREY, "* Ese jugador ya tiene permisos en tu casa, revisa slot 1."); return 1; }
	if (strmatch(InfoCasa[n][cCompartida2], InfoPersonaje[playerid2][pNombre_Apellido])) { SendClientMessage(playerid, COLOR_GREY, "* Ese jugador ya tiene permisos en tu casa, revisa slot 2."); return 1; }
	if (strmatch(InfoCasa[n][cCompartida3], InfoPersonaje[playerid2][pNombre_Apellido])) { SendClientMessage(playerid, COLOR_GREY, "* Ese jugador ya tiene permisos en tu casa, revisa slot 3."); return 1; }
	switch (ShareCasa[playerid])
	{
	    case 1: { alm(InfoCasa[n][cCompartida1], InfoPersonaje[playerid2][pNombre_Apellido]); guardar_string_mysql("casas", "Compartida1", InfoCasa[n][cID_sql], InfoCasa[n][cCompartida1]); }
	    case 2: { alm(InfoCasa[n][cCompartida2], InfoPersonaje[playerid2][pNombre_Apellido]); guardar_string_mysql("casas", "Compartida2", InfoCasa[n][cID_sql], InfoCasa[n][cCompartida2]); }
	    case 3: { alm(InfoCasa[n][cCompartida3], InfoPersonaje[playerid2][pNombre_Apellido]); guardar_string_mysql("casas", "Compartida3", InfoCasa[n][cID_sql], InfoCasa[n][cCompartida3]); }
	}
	SendClientMessageEx(playerid2, 0x59F37BFF, "* %s te ha dado permisos de su casa %d.", NombreOriginal(playerid), n);
	SendClientMessageEx(playerid, 0x59F37BFF, "* Cediste permisos de tu casa %d con %s (slot %d).", n, NombreOriginal(playerid2), ShareCasa[playerid]);
	return 1;
}

