VehicleSpawnCount(playerid)
{
	new cars = 0;
	Loop(id, totalvehiculos, 1) if (InfoVehiculo[id][vTipo] == 0 && strcmp(InfoPersonaje[playerid][pNombre_Apellido], InfoVehiculo[id][vPropietario], true) == 0 && InfoVehiculo[id][vSpawned] != 0)
	{ cars++; }
	return cars;
}

SpawnVehiculos(playerid)
{
	new carsamount = VehicleSpawnCount(playerid);
	if (InfoCuenta[playerid][jVIP] == 0 && carsamount >= 2)
	{
		SendClientMessage(playerid, COLOR_GREY, "* Usuarios normales sólo pueden tener 2 vehículos spawneados.");
		return 1;
	}
	if (InfoCuenta[playerid][jVIP] == 1 && carsamount >= 5)
	{
		SendClientMessage(playerid, COLOR_GREY, "* Usuarios VIP Esmeralda sólo pueden tener 5 vehículos spawneados.");
		return 1;
	}
	if (InfoCuenta[playerid][jVIP] == 2 && carsamount >= 10)
	{
		SendClientMessage(playerid, COLOR_GREY, "* Usuarios VIP Diamante sólo pueden tener 10 vehículos spawneados.");
		return 1;
	}
	return 2;
}

Dialog:Menu2Vehiculos(playerid, response, listitem, inputtext[])
{
	if (!response) return pc_cmd_llaves(playerid);
	new id = id_veh_actual[playerid], string[235], dialog[1428];
	if (InfoVehiculo[id][vModelo] == 0) return SendClientMessage(playerid, COLOR_GREY, "* El vehículo que seleccionaste no posee un modelo.");
 	if (InfoVehiculo[id][vEmbargo] != 0) return SendClientMessage(playerid, COLOR_GREY, "* El vehículo que seleccionaste se encuentra embargado.");
	switch (listitem)
	{
		case 0: //spawn-unspawn
		{
			if (InfoVehiculo[id][vSpawned] != 0) //ocultar
			{
				new Float: vx, Float: vy, Float: vz;
				GetVehiclePos(InfoVehiculo[id][vIG_id], vx, vy, vz);
				if (!IsPlayerInRangeOfPoint(playerid, 12, vx, vy, vz)) return SendClientMessage(playerid, COLOR_GREY, "* Debes estar en un rango de 12 metros de tu vehículo para ocultarlo.");
				if (IsVehicleBusy(InfoVehiculo[id][vIG_id])) return SendClientMessage(playerid, COLOR_GREY, "* No puedes ocultar el vehículo si posee ocupantes.");
				Quit_Vehicles(id);
				SendClientMessageEx(playerid, COLOR_YELLOW, "Has ocultado tu %s.", GetNameVehicle(InfoVehiculo[id][vModelo]));
			}
			else
			{
				if (SpawnVehiculos(playerid) == 1) return 1;
				if (InfoVehiculo[id][vEndPosicionX] != 0) Dialog_Show(playerid, ShowSpawnVeh, DIALOG_STYLE_LIST, "Lugar de mostrar", "Última posición\nSitio de aparcado", "Continuar", "Inicio");
				else { MostrarVehiculo(id, 2), SendClientMessageEx(playerid, COLOR_GREEN, "Has sacado tu %s en donde lo aparcaste.", GetNameVehicle(InfoVehiculo[id][vModelo])); }
			}
		}
		case 1: //localizar
		{
			if (InfoVehiculo[id][vSpawned] == 0) return SendClientMessage(playerid, COLOR_GREY, "* El vehículo que seleccionaste está oculto.");
			if (Granjero[playerid] != 0) return SendClientMessage(playerid, COLOR_GREY, "* Debes terminar de cosechar primero.");
			new vehicleid = InfoVehiculo[id][vIG_id], Float: carPos[3], zone[MAX_ZONE_NAME];
			GetVehiclePos(vehicleid, carPos[0], carPos[1], carPos[2]);
			GetCar3DZone(vehicleid, zone, sizeof zone);
			SendClientMessageEx(playerid, 0x0073E6FF, "Tu %s fue marcado en el radar. (%s)", GetNameVehicle(InfoVehiculo[id][vModelo]), zone);
			SetPlayerCheckpoint(playerid, carPos[0], carPos[1], carPos[2], 10.0);
			PlayerPlaySound(playerid, 1138, 0, 0, 0);
			SetPVarInt(playerid, "TrackCar", 1);
		}
		case 2: //información
		{
			format(string, sizeof string, "{FFFFFF}Modelo: {00AACC}%s {FFFFFF}({0073E6}%d{FFFFFF})\n", GetNameVehicle(InfoVehiculo[id][vModelo]), InfoVehiculo[id][vModelo]); strcat(dialog, string);
			format(string, sizeof string, "{FFFFFF}Propietario: {00AACC}%s\n", InfoVehiculo[id][vPropietario]); strcat(dialog, string);
			if (InfoVehiculo[id][vSpawned] != 0) format(string, sizeof string, "{FFFFFF}Gasolina: {00AACC}%d\n", veh_gasolina[InfoVehiculo[id][vIG_id]]), strcat(dialog, string);
			else format(string, sizeof string, "{FFFFFF}Gasolina: {00AACC}%d\n", InfoVehiculo[id][vGasolina]), strcat(dialog, string);
			format(string, sizeof string, "{FFFFFF}Valor: {00B000}$%s\n", diner_coma(InfoVehiculo[id][vPrecio])); strcat(dialog, string);
			if (InfoVehiculo[id][vMulta] != 0) { format(string, sizeof string, "{FFFFFF}Multas: {D70000}$%s\n", diner_coma(InfoVehiculo[id][vMulta])); strcat(dialog, string); }
			format(string, sizeof string, "{FFFFFF}Slots maletero: {00AACC}%d\n", InfoVehiculo[id][vMaxMaletero]); strcat(dialog, string);
			Dialog_Show(playerid, ShowInvalido, DIALOG_STYLE_MSGBOX, "Información del vehículo", dialog, "Cerrar", "");
		}
		case 3: //maletero
		{
			new count = 0;
			Loop(x, 10, 0) if (InfoVehiculo[id][vMaletero][x] != 0)
			{
				count++;
				format(string, sizeof string, "#%d %s", x+1, slot_objeto(InfoVehiculo[id][vMaletero][x], InfoVehiculo[id][vMaleteroCant][x]));
				SendClientMessage(playerid, COLOR_WHITE, string);
			}
			if (count == 0) return SendClientMessage(playerid, COLOR_GREY, "No hay nada en el maletero.");
			SendClientMessage(playerid, -1, "Recuerda que los comandos de maletero son: {00AACC}/gmaletero {FFFFFF}-{00AACC} /smaletero");
		}
		case 4: //eliminar
		{
			if (InfoVehiculo[id][vPremium] != 0) return SendClientMessage(playerid, COLOR_GREY, "No puedes eliminar vehículos que compraste con monedas.");
			format(dialog, sizeof dialog, "{EBEBEB}El uso de esta opción {9c2828}eliminará completamente{EBEBEB} tú {00AACC}%s{EBEBEB} y no habrá reembolso.\n\
			Si estás seguro de que quieres eliminar el vehículo, por favor presiona {9c2828}continuar{EBEBEB}.\n", GetNameVehicle(InfoVehiculo[id][vModelo]));
			Dialog_Show(playerid, EliminarVehiculo, DIALOG_STYLE_MSGBOX, "Desmantelar vehículo", dialog, "Continuar", "Inicio");
		}
		case 5: VehiculoShareShow(playerid);
	}
	return 1;
}

