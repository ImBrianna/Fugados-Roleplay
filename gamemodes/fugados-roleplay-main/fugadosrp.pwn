Dialog:Show2DNIFalso(playerid, response, listitem, inputtext[])
{
	if (!response) return Dialog_Show(playerid, ShowDNIFalso, DIALOG_STYLE_INPUT, "{00AACC}Documento falso", "{FFFFFF}Ingresa el nombre que quieres usar para el Documento falso, precio: {00CC00}$15.000", "Continuar", "Salir");
	new precio = 4800, string[190];
	if (GetPlayerCash(playerid) < precio)
	{
		format(string, sizeof string, "Te hacen falta $%s para comprar '{00AACC}Documento falso{EBEBEB}' por $%s.", diner_coma(precio-GetPlayerCash(playerid)), diner_coma(precio));
		SendClientMessage(playerid, 0xEBEBEBFF, string);
		return 1;
	}
	new dni = 100000 + random(899999);
	InfoPersonaje[playerid][pFalsoDocumento] = dni;
	new query[256];
	mysql_format(db_samp, query, sizeof query, "UPDATE personajes SET FalsoDocumento = '%d', FalsoNombre = '%e' WHERE ID = '%d'", InfoPersonaje[playerid][pFalsoDocumento], InfoPersonaje[playerid][pFalsoNombre], InfoPersonaje[playerid][pID]);
	mysql_tquery(db_samp, query);
	format(query, sizeof query, "* Compraste una identificación falsa (/dnifalso para mostrarlo) con el nombre de %s.", InfoPersonaje[playerid][pFalsoNombre]);
	SendClientMessage(playerid, COLOR_LIGHTGREEN, query);
	GivePlayerCash(playerid, -precio, "compra de dni falso");
	return 1;
}

