	if (sscanf(params, "d", vehicleid)) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /revisarmaletero (VehiculoID) [el de /dl]");
	if (vehicleid < 1 || vehicleid > 1999) return SendClientMessage(playerid, COLOR_GREY, "El vehículo seleccionado no es de ningún usuario o no existe. (2)");
	if (!CercaDelVehiculo(playerid, vehicleid, 9.0)) return SendClientMessage(playerid, COLOR_GREY, "* No estás cerca del vehículo especificado.");
	Loop(id, totalvehiculos, 1) if (InfoVehiculo[id][vIG_id] == vehicleid)
	{
		if (InfoVehiculo[id][vModelo] == 0) return SendClientMessage(playerid, COLOR_GREY, "Vehículo seleccionado no tiene modelo.");
		if (InfoVehiculo[id][vSpawned] == 0) return SendClientMessage(playerid, COLOR_GREY, "Vehículo seleccionado se encuentra oculto.");
		new count = 0;
		Loop(x, 10, 0) if (InfoVehiculo[id][vMaletero][x] != 0)
		{
			count++;
			new dex_query[170];
			InfoVehiculo[id][vMaletero][x] = 0;
			InfoVehiculo[id][vMaleteroCant][x] = 0;
			mysql_format(db_samp, dex_query, sizeof dex_query, "UPDATE vehiculos SET Maletero%d = 0, MaleteroCant%d = 0 WHERE ID = '%d'", x+1, x+1, InfoVehiculo[id][vID_sql]);
			mysql_tquery(db_samp, dex_query);
		}
		if (count == 0) return SendClientMessage(playerid, COLOR_GREY, "No hay nada que incautar del vehículo.");
		new gformat[190];
		format(gformat, sizeof gformat, "incauta los objetos del vehículo %s.", GetNameVehicle(InfoVehiculo[id][vModelo]));
		pc_cmd_me(playerid, gformat);
		return 1;
	}
	SendClientMessage(playerid, COLOR_GREY, "El vehículo seleccionado no existe o no tiene registro.");
	return 1;
}

