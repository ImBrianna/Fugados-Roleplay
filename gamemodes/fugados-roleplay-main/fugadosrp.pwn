cmd:revisartrailer(playerid, params[]){
	if (!member_faccion(playerid, 1, 4)) return SendClientMessage(playerid, COLOR_GREY, "Sólo miembros de LSPD, LSCSD.");
	new vehid, veh = -1;
	if (sscanf(params, "i", vehid)) return SendClientMessage(playerid, COLOR_GREY, "Utiliza: /revisartariler (trailerID) (carga)");
	if(!IsTrailerVehicle(vehid)) return SendClientMessage(playerid, COLOR_GREY, "* Este vehículo no es una carga de trailer");
	for(new i; i < MAX_TCARGA; i++){
		if(traInfo[i][VehID] == vehid) veh = i;
	}
	if(veh == -1) return SendClientMessage(playerid, COLOR_GREY, "* Esta carga no está vacía.");
	if(traInfo[veh][cTipo]  == 2) SendClientMessageEx(playerid, COLOR_GREY, "Dentro del camión te has encontrado con {9a092c}%s.", (traInfo[veh][Modelo] == 584) ? cTrailer[2][traInfo[veh][Tipo]] : cTrailer[3][traInfo[veh][Tipo]]);
	else if(traInfo[veh][cTipo] == 3) SendClientMessageEx(playerid, COLOR_GREY, "Dentro del camión te has encontrado con {9a092c}%s.", cTrailer[4][traInfo[veh][Tipo]]);
	else SendClientMessageEx(playerid, COLOR_GREY, "Dentro del camión te has encontrado con {4ab852}%s.", (traInfo[veh][Modelo] == 584) ? cTrailer[0][traInfo[veh][Tipo]] : cTrailer[1][traInfo[veh][Tipo]]);
	return 1;
}

