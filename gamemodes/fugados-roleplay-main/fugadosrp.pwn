cmd:verip(playerid, params[])
{
	if (CheckAdminCMD(playerid, 3)) return 1;
	new ip[18], playerid2;
	if (sscanf(params, "d", playerid2)) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /verip (JugadorID)");
	if (!IsPlayerConnectedEx(playerid2)) return SendClientMessage(playerid, COLOR_GREY, "Jugador desconectado.");
	if (CheckAdminCMD(playerid, InfoCuenta[playerid2][jAdmin])) return 1;
	GetPlayerIp(playerid2, ip, sizeof ip);
	SendClientMessageEx(playerid, COLOR_PURPLE, "%s [ID: %d] (Cuenta: %s [%d]) - IP: %s.", InfoPersonaje[playerid2][pNombre_Apellido], playerid2, InfoCuenta[playerid2][jNombre], InfoCuenta[playerid2][jID], ip);
	return 1;
}

cmd:bloquearip(playerid, params[])
{
	if (CheckAdminCMD(playerid, 2)) return 1;
	if (!CheckTimer(60, TimerCban[playerid]))
	{
		new ip[18], reason[64], string[190];
		if (sscanf(params, "s[18]s[64]", ip, reason)) return SendClientMessage(playerid, COLOR_GREEN, "Utiliza: /bloquearip (IP) (Razón)");
		if (IsValidIP(ip)) return SendClientMessage(playerid, COLOR_GREY, "Esa IP no es válida, intenta de nuevo.");
		if (CheckBan(ip) != 0) return SendClientMessage(playerid, COLOR_GREY, "* Esa IP ya se encuentra bloqueda.");
		format(string, sizeof string, "{FF6347}Administración:{FFFFFF} %s(%d) bloqueó la IP %s. Razón: %s", InfoCuenta[playerid][jNombre], playerid, ip, reason);
		SendAdminMessage(0xFFFFFFFF, string);
		//register ban ip
		AddBan(ip, InfoCuenta[playerid][jNombre], reason);
		//seguridad 24/02/2020
		CountBan[playerid]++;
		if (CountBan[playerid] == 5)
		{
			TimerCban[playerid] = gettime();
			CountBan[playerid] = 0;
		}
	}
	else
	{
		SendClientMessageEx(playerid, COLOR_GREY, "Puedes volver a usar este comando en %s.", Segundos_tiempo( CheckTimer(180, TimerCban[playerid]) ));
	}
	return 1;
}

