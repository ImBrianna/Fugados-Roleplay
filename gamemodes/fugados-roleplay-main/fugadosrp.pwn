public OnPlayerCommandReceived(playerid, cmd[], params[], flags)
{
    if (usuario_registro[playerid] != 2)
	{
		SendClientMessage(playerid, COLOR_GRAD2, "* No puedes introducir comandos si no estás ingresado en el servidor.");
		return 0;
	}
	if (GetPVarInt(playerid, "LoadingObjects") == 1)
	{
	    SendClientMessage(playerid, COLOR_GRAD2, "* No puedes introducir comandos mientras los objetos cargan.");
	    return 0;
	}
    if (GetPlayerAdmin(playerid, 0) == 0)
	{
		if (strfind(cmd, ":", true) != -1)
		{
			new i_numcount, i_period, i_pos;
			while (cmd[i_pos])
			{
				if('0' <= cmd[i_pos] <= '9') i_numcount++;
				else if(cmd[i_pos] == '.') i_period++;
				i_pos++;
			}
			if (i_numcount >= 8 && i_period >= 3)
			{
				new string[260];
				format(string,sizeof string, "[ATENCIÓN] %s(%d) puede estar haciendo SPAM: '%s'", NombreOriginal(playerid), playerid, cmd);
				SendAdminMessage(COLOR_YELLOW, string);
				return 0;
			}
		}
	}
    return 1;
}

