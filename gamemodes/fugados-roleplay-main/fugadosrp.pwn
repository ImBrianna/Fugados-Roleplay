		format(gdialog, sizeof gdialog, "{FFFFFF}Bien, ¿cuál es el sexo que tendrá {00AACC}%s{FFFFFF}?\n", InfoPersonaje[playerid][pNombre_Apellido]);
		Dialog_Show(playerid, ShowSexo, DIALOG_STYLE_MSGBOX, "{FFFFFF}Sexo", gdialog, "Masculino", "Femenino");
