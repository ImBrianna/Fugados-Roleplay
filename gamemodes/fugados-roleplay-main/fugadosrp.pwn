		}
		else
		{
			SendClientMessage(playerid, COLOR_GREEN, "Saliste del tractor, el mismo volvió a su posición original.");
			DisableRemoteVehicleCollisions(playerid, false);
		}
	}
	veh_asiento[playerid] = -1;
	return 1;
}

